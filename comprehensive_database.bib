	journal = {Human Ecology},
	keywords = {Conservation, Conservation biology, Human behavior},
	month = {Dec},
	number = {4},
	pages = {355--387},
	title = {Testing the "ecologically noble savage'' hypothesis: Interspecific prey choice by Piro hunters of Amazonian Peru},
	url = {https://link.springer.com/content/pdf/10.1007/BF00891140.pdf},
	volume = {21},
