	keywords = {Home range, Kernel density estimator, Species distribution models},
	number = {1},
	pages = {164-168},
	title = {Kernel Methods for Estimating the Utilization Distribution in Home-Range Studies},
	url = {https://esajournals.onlinelibrary.wiley.com/doi/abs/10.2307/1938423},
	volume = {70},
