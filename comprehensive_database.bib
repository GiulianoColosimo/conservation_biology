	doi = {https://doi.org/10.1046/j.1523-1739.1999.98115.x},
	eprint = {https://conbio.onlinelibrary.wiley.com/doi/pdf/10.1046/j.1523-1739.1999.98115.x},
	journal = {Conservation Biology},
	keywords = {Inbreeding depression, Habitat fragmentation, Isolated populations, Gene flow},
	number = {3},
	pages = {531-541},
	title = {Unprecedented Low Levels of Genetic Variation and Inbreeding Depression in an Island Population of the Black-Footed Rock-Wallaby},
	url = {https://conbio.onlinelibrary.wiley.com/doi/abs/10.1046/j.1523-1739.1999.98115.x},
	volume = {13},
