@article{Waples2013a,
	abstract = {Although standard statistical tests (such as contingency chi-square or G tests) are not well suited to the analysis of temporal changes in allele frequencies, they continue to be used routinely in this context. Because the null hypothesis stipulated by the test is violated if samples are temporally spaced, the true probability of a significant test statistic will not equal the nominal a level, and conclusions drawn on the basis of such tests can be misleading. A generalized method, applicable to a wide variety of organisms and sampling schemes, is developed here to estimate the probability of a significant test statistic if the only forces acting on allele frequencies are stochastic ones (i.e., sampling error and genetic drift). Results from analyses and simulations indicate that the rate at which this probability increases with time is determined primarily by the ratio of sample size to effective population size. Because this ratio differs considerably among species, the seri- ousness of the error in using the standard test will also differ. Bias is particularly strong in cases in which a high percentage of the total population can be sampled (for example, endangered species). The model used here is also applicable to the analysis of parent-offspring data and to comparisons of replicate samples from the same generation. A generalized test of the hypothesis that observed changes in allele frequency can be satisfactorily explained by drift follows directly from the model, and simulation results indicate that the true a level of this adjusted test is close to the nominal one under most condition},
	author = {Waples, Robin S},
	date-added = {2021-07-01 12:00:14 +0200},
	date-modified = {2021-07-01 12:00:16 +0200},
	doi = {10.2307/2409359},
	file = {:Users/giuliano/Library/Application Support/Mendeley Desktop/Downloaded/Waples - 2013 - Temporal variation in allele frequencies testing the right hypothesis.pdf:pdf},
	isbn = {0014-3820},
	issn = {0014-3820},
	journal = {Evolution},
	number = {6},
	pages = {1236--1251},
	title = {{Temporal variation in allele frequencies: testing the right hypothesis}},
	volume = {43},
	year = {2013},
	bdsk-url-1 = {https://doi.org/10.2307/2409359}}

@article{Waples1998,
	abstract = {Taxonomy and systematics have long been characterized by heated debates$\backslash$nover methodology and pointed exchanges between "lumpers" and "splitters."$\backslash$nTherefore, when I wrote the paper outlining the concept of evolutionarily$\backslash$nsignificant units (ESUs) for identifying "distinct population segments"$\backslash$nof Pacific salmon (Oncorhynchus spp.) under the U.S. Endangered Species$\backslash$nAct (ESA), I realized that any attempt to identify conservation units$\backslash$nbelow the species level would also generate a good deal of scientific$\backslash$ndiscussion. This has proved to be the case. At the same time, a parallel$\backslash$ndiscussion has taken place of which the primary focus is not biological$\backslash$nissues but rather the degree to which the ESU concept satisfies the$\backslash$noriginal intent of the U.S. Congress and the ESA itself. The recent$\backslash$nessay in these pages by Pennock and Dimmick (1997) follows in this$\backslash$ntradition. Pennock and Dimmick argue that the ESU concept adopted$\backslash$nas policy by the National Marine Fisheries Service for identifying$\backslash$ndistinct population segments of Pacific salmon under the ESA (1)$\backslash$nis inconsistent with the original intent of Congress, (2) unduly$\backslash$nnarrows the scope of populations that can be considered distinct$\backslash$nunder the ESA, and (3) limits flexibility under the ESA. The comments$\backslash$nbelow address these criticisms as they apply to the NMFS ESU policy$\backslash$nfor salmon.},
