	file = {:Users/giuliano/Library/Application Support/Mendeley Desktop/Downloaded/Wirth et al. - 2004 - Distinguishing human ethnic groups by means of sequences from Helicobacter pylori lessons from Ladakh.pdf:pdf},
	isbn = {0027-8424 (Print)$\backslash$n0027-8424 (Linking)},
