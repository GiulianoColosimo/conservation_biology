@article{Rambaut2018,
	abstract = {Bayesian inference of phylogeny using Markov chain Monte Carlo (MCMC) plays a central role in understanding evolutionary history from molecular sequence data. Visualizing and analyzing the MCMC-generated samples from the posterior distribution is a key step in any non-trivial Bayesian inference. We present the software package Tracer (version 1.7) for visualizing and analyzing the MCMC trace files generated through Bayesian phylogenetic inference. Tracer provides kernel density estimation, multivariate visualization, demographic trajectory reconstruction, conditional posterior distribution summary, and more.},
	author = {Rambaut, Andrew and Drummond, Alexei J. and Xie, Dong and Baele, Guy and Suchard, Marc A.},
	date-added = {2021-07-01 12:00:14 +0200},
