	keywords = {Ticks, Ixodidae, Michoac{\'{a}}n, Geographic Distributio},
	number = {October},
	pages = {467--477},
	title = {{Potential Species Distribution and Richness of Ixodidae Ticks Associated with Wild Vertebrates from Michoac{\'{a}}n , Mexico}},
	year = {2014}}

