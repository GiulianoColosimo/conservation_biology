	file = {:Users/giuliano/Library/Application Support/Mendeley Desktop/Downloaded/Faubet, Waples, Gaggiotti - 2007 - Evaluating the performance of a multilocus Bayesian method for the estimation of migration rates.pdf:pdf},
	issn = {0962-1083},
	journal = {Molecular Ecology},
	keywords = {Algorithms,Bayes Theorem,Computer Simulation,Emigration and Immigration,Genetic Markers,Genetic Markers: genetics,Genetic Variation,Inbreeding,Models,Statistical,bayesian methods,gene flow,mcmc,migration,multilocus genotypes,parameter},
	number = {6},
	pages = {1149--1166},
	pmid = {17391403},
	title = {{Evaluating the performance of a multilocus Bayesian method for the estimation of migration rates}},
	url = {http://onlinelibrary.wiley.com/doi/10.1111/j.1365-294X.2007.03218.x/full{\%}5Cnhttp://www.ncbi.nlm.nih.gov/pubmed/17391403},
	volume = {16},
