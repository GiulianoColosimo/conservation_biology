	file = {:Users/giuliano/Library/Application Support/Mendeley Desktop/Downloaded/Evanno, Regnaut, Goudet - 2005 - Detecting the number of clusters of individuals using the software STRUCTURE A simulation study.pdf:pdf},
	isbn = {0962-1083 (Print)$\backslash$n0962-1083 (Linking)},
	issn = {09621083},
	journal = {Molecular Ecology},
	keywords = {AFLP,Hierarchical structure,Microsatellite,STRUCTURE software,Simulations},
	number = {8},
	pages = {2611--2620},
	pmid = {15969739},
	title = {{Detecting the number of clusters of individuals using the software STRUCTURE: A simulation study}},
