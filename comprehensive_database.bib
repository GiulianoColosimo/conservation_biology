	isbn = {0006-3207, 0006-3207},
	issn = {00063207},
	journal = {Biological Conservation},
	keywords = {Extinction risk,Genetic diversity,Inbreeding depression,Mutation accumulation,Self-incompatibility},
	number = {2},
	pages = {131--140},
	pmid = {4245},
	title = {{Genetics and extinction}},
	volume = {126},
	year = {2005},
