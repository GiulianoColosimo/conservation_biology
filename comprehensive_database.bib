	journal = {Heredity},
	keywords = {Alleles,Finland,Gene Frequency,Genetic Variation,Geography,Host-Parasite Interactions,Isoenzymes,Isoenzymes: genetics,Solanaceae,Solanaceae: genetics,Solanaceae: physiology,Urtica dioica,Urtica dioica: genetics,Urtica dioica: parasitology},
	number = {4},
	pages = {318--24},
	pmid = {12242649},
	title = {{Population structure of a parasitic plant and its perennial host.}},
	url = {http://www.ncbi.nlm.nih.gov/pubmed/12242649},
