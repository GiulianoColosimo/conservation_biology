	issn = {0016-6731},
	journal = {Genetics},
	keywords = {Alcohol Dehydrogenase,Alcohol Dehydrogenase: genetics,Algorithms,Amylases,Amylases: genetics,Animals,Carboxylesterase,Carboxylic Ester Hydrolases,Carboxylic Ester Hydrolases: genetics,Drosophila melanogaster,Drosophila melanogaster: enzymology,Drosophila melanogaster: genetics,Genetic Techniques,Genetic Techniques: statistics {\&} numerical data,Haplotypes,Mutation,Phenotype,Recombination, Genetic,Restriction Mapping,Sequence Analysis, DNA},
	number = {2},
	pages = {619--33},
	pmid = {1385266},
	title = {{A cladistic analysis of phenotypic associations with haplotypes inferred from restriction endonuclease mapping and DNA sequence data. III. Cladogram estimation.}},
	url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=1205162{\&}tool=pmcentrez{\&}rendertype=abstract},
	volume = {132},
