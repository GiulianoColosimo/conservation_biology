	bdsk-url-2 = {https://doi.org/10.1111/mec.12459}}

@article{Dubey2012,
	abstract = {BACKGROUND: To conserve critically endangered predators, we also need to conserve the prey species upon which they depend. Velvet geckos (Oedura lesueurii) are a primary prey for the endangered broad-headed snake (Hoplocephalus bungaroides), which is restricted to sandstone habitats in southeastern Australia. We sequenced the ND2 gene from 179 velvet geckos, to clarify the lizards' phylogeographic history and landscape genetics. We also analysed 260 records from a longterm (3-year) capture-mark-recapture program at three sites, to evaluate dispersal rates of geckos as a function of locality, sex and body size.$\backslash$n$\backslash$nRESULTS: The genetic analyses revealed three ancient lineages in the north, south and centre of the species' current range. Estimates of gene flow suggest low dispersal rates, constrained by the availability of contiguous rocky habitat. Mark-recapture records confirm that these lizards are highly sedentary, with most animals moving {\textless} 30 m from their original capture site even over multi-year periods.$\backslash$n$\backslash$nCONCLUSION: The low vagility of these lizards suggests that they will be slow to colonise vacant habitat patches; and hence, efforts to restore degraded habitats for broad-headed snakes may need to include translocation of lizards.},
	author = {Dubey, Sylvain and Croak, Benjamin and Pike, David and Webb, Jonathan and Shine, Richard},
	date-added = {2021-07-01 12:00:14 +0200},
	date-modified = {2021-07-01 12:00:14 +0200},
	doi = {10.1186/1471-2148-12-67},
	file = {:Users/giuliano/Library/Application Support/Mendeley Desktop/Downloaded/Dubey et al. - 2012 - Phylogeography and dispersal in the velvet gecko (iOedura lesueuriii), and potential implications for conservation.pdf:pdf},
	isbn = {1471-2148},
	issn = {1471-2148},
	journal = {BMC Evolutionary Biology},
	keywords = {australia,conservation,dispersal,landscape genetics,phylogeography,reptile},
	number = {1},
	pages = {67},
