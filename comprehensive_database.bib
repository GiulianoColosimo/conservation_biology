	journal = {Proceedings of the National Academy of Sciences of the United States of America},
	number = {14},
	pages = {4746--4751},
	pmid = {15051885},
	title = {{Distinguishing human ethnic groups by means of sequences from {\textless}i{\textgreater}Helicobacter pylori{\textless}/i{\textgreater}: lessons from Ladakh.}},
