@article{Calsbeek2003,
	abstract = {Islands are considered to be natural laboratories in which to examine evolution because of the implicit assumption that limited gene flow allows tests of evolutionary processes in isolated replicates. Here we show that this well-accepted idea requires re-examination. Island inundation during hurricanes can have devastating effects on lizard populations in the Bahamas. After severe storms, islands may be recolonized by over-water dispersal of lizards from neighbouring islands. High levels of gene flow may homogenize genes responsible for divergence, and are widely viewed as a constraining force on evolution. Ultimately, the magnitude of gene flow determines the extent to which populations diverge from one another, and whether or not they eventually form new species. We show that patterns of gene flow among island populations of Anolis lizards are best explained by prevailing ocean currents, and that over-water dispersal has evolutionary consequences. Across islands, divergence in fitness-related morphology decreases with increasing gene flow. Results suggest that over-water dispersal after hurricanes constrains adaptive diversification in Anolis lizards, and that it may have an important but previously undocumented role in this classical example of adaptive radiation.},
	author = {Calsbeek, Ryan and Smith, Thomas B},
	date-added = {2021-07-01 12:00:14 +0200},
	date-modified = {2021-07-01 12:00:14 +0200},
	doi = {10.1038/nature02143},
	isbn = {0028-0836$\backslash$r1476-4679},
	issn = {0028-0836},
	journal = {Nature},
	keywords = {Evolution,Islands},
	mendeley-tags = {Evolution,Islands},
	number = {6966},
	pages = {552--555},
	pmid = {14654839},
	title = {{Ocean currents mediate evolution in island lizards.}},
	volume = {426},
	year = {2003},
	bdsk-url-1 = {https://doi.org/10.1038/nature02143}}

@article{Campbell2008,
	abstract = {Microhabitat selection by reptiles can be affected by a complex interplay of abiotic and biotic factors. The Brown Treesnake (Boiga irregularis) is an efficient nocturnal predator that hunts in the tropical forest canopy and on the ground, using visual and chemical cues. To identify the factors influencing Brown Treesnake microhabitat use, we experimentally manipulated an abiotic factor, moonlight level, and a biotic factor, prey presence. We hypothesized that (1) moonlight would affect microhabitat use and (2) the presence of prey would alter microhabitat use in various moonlight levels. Trials were conducted in a large laboratory chamber with artificial trees in simulated new, half, and full moonlight. In each trial, the snake's location in canopy, subcanopy, or open ground was recorded at 60-sec intervals for 100 min. Treesnake microhabitat use was determined in three moonlight levels without prey present and in two moonlight levels with a mouse (adult Mus musculus) or a Mangrove Monitor (juvenile Varanus indicus) present. The treesnakes used open ground areas more as moonlight decreased, and they used the canopy more as moonlight increased. No significant differences existed within a moonlight level between trials with or without prey. Thus, moonlight appeared to supercede prey availability in affecting Brown Treesnake microhabitat use. Additionally, the effect of nocturnal illumination on Brown Treesnake habitat use may also have important conservation implications regarding trapping techniques and deterring the snakes from specific areas.},
	author = {Campbell, Steven R. and Mackessy, Stephen P. and Clarke, Jennifer a.},
	date-added = {2021-07-01 12:00:14 +0200},
	date-modified = {2021-07-01 12:00:14 +0200},
	doi = {10.1670/07-0681.1},
	file = {:Users/giuliano/Library/Application Support/Mendeley Desktop/Downloaded/Campbell, Mackessy, Clarke - 2008 - Microhabitat Use by Brown Treesnakes (Boiga irregularis) Effects of Moonlight and Prey.pdf:pdf},
	issn = {0022-1511},
	journal = {Journal of Herpetology},
	keywords = {Reptiles},
	mendeley-tags = {Reptiles},
	number = {2},
	pages = {246--250},
	title = {{Microhabitat Use by Brown Treesnakes (Boiga irregularis): Effects of Moonlight and Prey}},
	volume = {42},
	year = {2008},
	bdsk-url-1 = {https://doi.org/10.1670/07-0681.1}}

