	issn = {00221503},
	journal = {Journal of Heredity},
	keywords = {Great Lakes,Habitat fragmentation,Microsatellites,Population genetics,Population structure,Rodentia,Small mammals},
	mendeley-tags = {Habitat fragmentation,Microsatellites,Population structure},
	number = {1},
	pages = {71--79},
	pmid = {21976772},
	title = {{Microsatellite genetic structure and cytonuclear discordance in naturally fragmented populations of deer mice (peromyscus maniculatus)}},
