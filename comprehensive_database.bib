	journal = {Evolutionary Applications},
	keywords = {Adaptation, Experimental evolution, Genetic inference, Population size, Population structure},
	number = {1},
	pages = {84--94},
	title = {{Conservation genetics as applied evolution: from genetic pattern to evolutionary process}},
	url = {http://doi.wiley.com/10.1111/j.1752-4571.2007.00008.x},
