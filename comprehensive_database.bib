	journal = {Frontiers in Ecology and the Environment},
	keywords = {Invasive species, Zebra mussel, Dreissena polymorpha},
	number = {3},
	pages = {135-141},
	title = {Twenty years of zebra mussels: lessons from the mollusk that made headlines},
