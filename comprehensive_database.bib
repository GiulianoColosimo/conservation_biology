	number = {3},
	pages = {298--306},
	pmid = {3194},
	title = {{Molecular phylogeny and biogeography of West Indian Teiid Lizards of the Genus Ameiva}},
	volume = {39},
	year = {2003},
	bdsk-url-1 = {https://doi.org/10.1016/j.ympev.2005.12.008}}

