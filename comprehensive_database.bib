	file = {:Users/giuliano/Library/Application Support/Mendeley Desktop/Downloaded/Lanterbecq et al. - 2010 - Genetic differentiation between marine iguanas from different breeding sites on the island of Santa F{\'{e}} (Gal.pdf:pdf},
	isbn = {0022-1503},
	issn = {00221503},
	journal = {Journal of Heredity},
	keywords = {Gal{\'a}pagos,dispersal,fine-scale genetic diversity,gene flow,marine iguanas,microsatellite,mtDNA},
	number = {6},
	pages = {663--675},
	pmid = {20538757},
