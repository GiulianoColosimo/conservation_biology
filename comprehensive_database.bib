	doi = {10.1111/ecog.01021},
	file = {:Users/giuliano/Dropbox/manuscripts/literature/PDFs/Raymond{\_}Important marine habitat off east antarctica. revealed by two decades of multi-species predator tracking{\_}2015.pdf:pdf},
	issn = {09067590},
	journal = {Ecography},
	keywords = {Remote sensing,Telemetry},
	mendeley-tags = {Remote sensing,Telemetry},
	month = {feb},
	number = {2},
