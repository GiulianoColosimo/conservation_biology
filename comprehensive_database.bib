@article{Swei2011,
	abstract = {The distribution of vector meals in the host community is an important element of understanding and predicting vector-borne disease risk. Lizards (such as the western fence lizard; Sceloporus occidentalis) play a unique role in Lyme disease ecology in the far-western United States. Lizards rather than mammals serve as the blood meal hosts for a large fraction of larval and nymphal western black-legged ticks (Ixodes pacificus-the vector for Lyme disease in that region) but are not competent reservoirs for the pathogen, Borrelia burgdorferi. Prior studies have suggested that the net effect of lizards is to reduce risk of human exposure to Lyme disease, a hypothesis that we tested experimentally. Following experimental removal of lizards, we documented incomplete host switching by larval ticks (5.19{\%}) from lizards to other hosts. Larval tick burdens increased on woodrats, a competent reservoir, but not on deer mice, a less competent pathogen reservoir. However, most larvae failed to find an alternate host. This resulted in significantly lower densities of nymphal ticks the following year. Unexpectedly, the removal of reservoir-incompetent lizards did not cause an increase in nymphal tick infection prevalence. The net result of lizard removal was a decrease in the density of infected nymphal ticks, and therefore a decreased risk to humans of Lyme disease. Our results indicate that an incompetent reservoir for a pathogen may, in fact, increase disease risk through the maintenance of higher vector density and therefore, higher density of infected vectors.},
	author = {Swei, A. and Ostfeld, R. S. and Lane, R. S. and Briggs, C. J.},
	date-added = {2021-07-01 12:00:14 +0200},
	date-modified = {2021-07-01 12:00:16 +0200},
	doi = {10.1098/rspb.2010.2402},
	file = {:Users/giuliano/Library/Application Support/Mendeley Desktop/Downloaded/Swei et al. - 2011 - Impact of the experimental removal of lizards on Lyme disease risk.pdf:pdf},
	isbn = {0962-8452},
	issn = {0962-8452},
	journal = {Proceedings of the Royal Society B: Biological Sciences},
	keywords = {borrelia burgdorferi,disease ecology,host composition,host preference,tick burden},
	number = {1720},
	pages = {2970--2978},
	pmid = {21325326},
	title = {{Impact of the experimental removal of lizards on Lyme disease risk}},
	url = {http://rspb.royalsocietypublishing.org/cgi/doi/10.1098/rspb.2010.2402},
	volume = {278},
