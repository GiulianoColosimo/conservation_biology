	doi = {10.1093/zoolinnean/zlx075},
	file = {:Users/giuliano/Dropbox/Manuscripts/Litterature/PDFs/Baeckens{\_}Sexual selection and the chemical signal design of lacertid lizards{\_}2017.pdf:pdf},
	issn = {10963642},
	journal = {Zoological Journal of the Linnean Society},
	keywords = {Lacertidae,animal communication,femoral gland secretions,lizards,phylogenetic comparative methods,sexual size dimorphism},
	number = {2},
	pages = {445--457},
	title = {{Sexual selection and the chemical signal design of lacertid lizards}},
	volume = {183},
