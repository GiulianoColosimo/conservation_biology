	file = {:Users/giuliano/Dropbox/manuscripts/literature/PDFs/Rivas-Torres{\_}A methodology for mapping native and invvasive vegetation cover in archipelagoes An example from the Gal{\'{a}}pagos islands{\_}2018.pdf:pdf},
	issn = {0309-1333},
	journal = {Progress in Physical Geography: Earth and Environment},
	keywords = {Drones,GIS, Gal{\'a}pagos, Gal{\'a}pagos National Park,Landsat 8,UAVs,invasive species,native and endemic species,vegetation map},
	mendeley-tags = {GIS,Gal{\'{a}}pagos,vegetation map},
