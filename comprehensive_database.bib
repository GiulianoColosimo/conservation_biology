	file = {:Users/giuliano/Library/Application Support/Mendeley Desktop/Downloaded/Strobel et al. - 2016 - Geographic and host-mediated population genetic structure in a cestode parasite of the three-spined stickleback.pdf:pdf},
	issn = {10958312},
	journal = {Biological Journal of the Linnean Society},
	keywords = {Alaska,Gasterosteus aculeatus,Schistocephalus solidus,admixture,genetic structure,host specificity,host--pathogen interaction},
