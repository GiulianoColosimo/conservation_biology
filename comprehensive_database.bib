	isbn = {1095-8649},
	issn = {00221112},
	journal = {Journal of Fish Biology},
	keywords = {Climate change,Density dependence,Growth rate,Individual yield,Modelling,Temperature,Von Bertalanffy},
	mendeley-tags = {Climate change,Density dependence,Growth rate},
	number = {7},
	pages = {1874--1889},
