	journal = {Journal of Biogeography},
	keywords = {Fleas,Host species richness,Parasite species richness,Small mammals},
	number = {11},
	pages = {1857--1866},
	title = {{Relationship between host diversity and parasite diversity: Flea assemblages on small mammals}},
	volume = {31},
