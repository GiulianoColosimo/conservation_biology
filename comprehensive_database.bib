	isbn = {1543-592X},
	issn = {1543-592X},
	journal = {Annual Review of Ecology, Evolution, and Systematics},
	number = {1},
	pages = {433--458},
	pmid = {243038500016},
	title = {{Limits to the Adaptive Potential of Small Populations}},
	volume = {37},
	year = {2006},
