	issn = {20457758},
	journal = {Ecology and Evolution},
	keywords = {R,R-packages,animal tracking,habitat selection,home range,movement ecology,resource-selection function,step-selection function,telemetry},
	mendeley-tags = {R,R-packages,animal tracking},
