@article{Lynch1990,
	abstract = {-Previous attempts to model the joint action of selection and mutation in finite popu- lations have treated population size as being independent of the mutation load. However, the accumulation of deleterious mutations is expected to cause a gradual reduction in population size. Consequently, in small populations random genetic drift will progressively overpower selection making it easier to fix future mutations. This synergistic interaction, which we refer to as a mu- tational melt-down, ultimately leads to population extinction. For many conditions, the coefficient of variation of extinction time is less than 0.1, and for species that reproduce by binary fission, the expected extinction time is quite insensitive to population carrying capacity. These results are consistent with observations that many cultures of ciliated protozoans and vertebrate fibroblasts have characteristic extinction times. The model also predicts that clonal lineages are unlikely to survive more than 104 to 105 generations, which is consistent with existing data on parthenogenetic animals. Contrary to the usual view that Muller's ratchet does more damage when selection is weak, we show that the mean extinction time declines as mutations become more deleterious. Although very small sexual populations, such as self-fertilized lines, are subject to mutational melt- downs, recombination effectively eliminates the process when the effective population size exceeds a dozen or so. The concept of the effective mutation load is developed, and several procedures for estimating it are described. It is shown that this load can be reduced substantially when mutational effects are highly variable.},
	author = {Lynch, Michael and Gabriel, Wilfried},
	date-added = {2021-07-01 12:00:14 +0200},
	date-modified = {2021-07-01 12:00:15 +0200},
	doi = {10.2307/2409502},
	file = {:Users/giuliano/Library/Application Support/Mendeley Desktop/Downloaded/Lynch, Gabriel - 1990 - Mutation Load and the Survival of Small Populations.pdf:pdf},
	isbn = {00143820},
	issn = {00143820},
	journal = {Evolution},
	number = {7},
	pages = {1725--1737},
	title = {{Mutation Load and the Survival of Small Populations}},
	volume = {44},
	year = {1990},
	bdsk-url-1 = {https://doi.org/10.2307/2409502}}

@book{Lynch1998,
	abstract = {The aim of this textbook is to bring together a diverse array of topics on the theory and applications of quantitative genetics in a way that is comprehensible to readers with a rudimentary understanding of statistics and genetics. It is the first of 2 volumes to be published on the subject, the focus of this volume being the basic biology and methods of analysis of quantitative traits. The second volume will include consideration of evolutionary aspects of quantitative genetics. The present volume draws examples from both the animal and plant kingdoms. There are 27 chapters and 5 appendices. These include consideration of the knowledge of statistical distributions, covariance, regression, correlation, matrix algebra and linear models that is necessary for an understanding of the quantitative genetics of animals and plants. The chapters are arranged in 3 large sections headed (1) Foundations of quantitative genetics, (2) Quantitative trait loci, and (3) Estimation procedures. The appendices cover (1) expectations, variances and covariances of compound variables, (2) path analysis, (3) further topics in matrix algebra and linear models, (4) maximum likelihood estimation, and (5) the computing power of statistical tests. There are indexes of authors, organisms and traits, and subjects.},
	address = {Sunderland, MA},
	author = {Lynch, Michael and Walsh, Bruce},
	booktitle = {Sinauer Associates},
	date-added = {2021-07-01 12:00:14 +0200},
	date-modified = {2021-07-01 12:00:15 +0200},
