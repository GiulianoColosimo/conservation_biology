	isbn = {1744-9561},
	issn = {1744-957X},
	journal = {Biology letters},
	keywords = {Adaptation,Animals,Astacoidea,Astacoidea: physiology,Biological,Biological: genetics,Biological: physiology,Environment,Fresh Water,Genetics,Inbreeding,Inbreeding depression,IxE,Models,Pennsylvania,Phenotypic plasticity,Population,Predatory Behavior,Predatory Behavior: physiology,Snails,Snails: genetics,Snails: physiology,Statistical},
