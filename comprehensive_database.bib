	issn = {0962-8452},
	journal = {Proceedings of the Royal Society B: Biological Sciences},
	keywords = {Fitness,Heterozygosity Fitness Correlation,breeding success,fitness correlation,heterozygosity,inbreeding,long-lived species,mhc},
