	file = {:Users/giuliano/Library/Application Support/Mendeley Desktop/Downloaded/Pontius et al. - 2007 - Initial sequence and comparative analysis of the cat genome.pdf:pdf},
	isbn = {1088-9051 (Print)$\backslash$n1088-9051 (Linking)},
	issn = {10889051},
