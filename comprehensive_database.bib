	issn = {0022-1511},
	journal = {Journal of Herpetology},
	number = {2},
	pages = {241--247},
	title = {{Range-Wide Genetic Analysis of the Threatened Bluetail Mole Skink Identifies Similar Genetic Structure with Sympatric Lizards}},
