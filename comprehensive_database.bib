	doi = {10.1073/pnas.1314702110},
	file = {:Users/giuliano/Library/Application Support/Mendeley Desktop/Downloaded/Vonk et al. - 2013 - The king cobra genome reveals dynamic gene evolution and adaptation in the snake venom system.pdf:pdf},
	isbn = {0027-8424},
	issn = {1091-6490},
	journal = {Proceedings of the National Academy of Sciences of the United States of America},
	keywords = {Adaptation, Biological,Adaptation, Biological: physiology,Animals,Cobra Venoms,Cobra Venoms: genetics,Cobra Venoms: metabolism,Elapidae,Elapidae: genetics,Elapidae: metabolism,Evolution, Molecular,Exocrine Glands,Exocrine Glands: metabolism,Genome,Genome: physiology,MicroRNAs,MicroRNAs: genetics,MicroRNAs: metabolism,Transcriptome,Transcriptome: physiology},
