@article{Wallace1975,
	abstract = {The concepts of hard and soft selection (Wallace, 1968, 1970) have been reviewed. Juvenile mortality as well as rigid and flexible selection (Dobzhansky, 1962, 1970) have been shown to be unrelated to the ideas illustrated by the published examples of soft selection. The following definitions are suggested: hard selection is both density and frequency independent; soft selection is both density and frequency de- pendent. Under this classification, selection that is now referred to as "density-depen- dent" would be frequency-independent while that known as "frequency-dependent" would be density-independent (Fig. 1). An analysis of an ecological model presented by Clarke (1973a, b) reveals that under certain conditions a-, k-, and w-selection all qualify as soft selection. Clarke's a-selection is always soft; his k-selection may at times be density-dependent only.},
	author = {Wallace, B},
	date-added = {2021-07-01 12:00:14 +0200},
