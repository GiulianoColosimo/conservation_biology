	isbn = {1573316393},
	issn = {00778923},
	journal = {Annals of the New York Academy of Sciences},
	keywords = {Ixodes ricinus,Poland,Rickettsia sp.,Rickettsiae,Spotted fever group},
	pages = {512--514},
	pmid = {17114767},
	title = {{The occurrence of spotted fever group (SFG) rickettsiae in Ixodes ricinus ticks (Acari: Ixodidae) in northern Poland}},
