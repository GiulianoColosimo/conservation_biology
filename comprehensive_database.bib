	keywords = {Animals,Brazil,Brazil: epidemiology,Endemic Diseases,Rickettsia,Rickettsia: classification,Rickettsia: isolation {\&} purification,Rocky Mountain Spotted Fever,Rocky Mountain Spotted Fever: epidemiology,Ticks,Ticks: microbiology},
