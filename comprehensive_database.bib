	keywords = {Conservation, Biodiversity, Ecosystem processes, Ecosystem services, Human-modified landscapes},
	month = {Aug},
	number = {9},
	pages = {2345--2369},
	title = {Supporting conservation with biodiversity research in sub-Saharan Africa's human-modified landscapes},
