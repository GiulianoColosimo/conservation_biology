	issn = {1744-9561},
	journal = {Biology letters},
	keywords = {climate change,habitat fragmentation},
	number = {1},
	pages = {152--154},
	pmid = {17148351},
	title = {{Genetic diversity in butterflies: Interactive effects of habitat fragmentation and climate-driven range expansion.}},
	volume = {2},
	year = {2006},
