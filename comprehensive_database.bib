@article{Parks2014,
	abstract = {The abundance of butternut (Juglans cinerea L.) trees has severely$\backslash$ndeclined rangewide over the past 50 years. An important factor in the$\backslash$ndecline is butternut canker, a disease caused by the fungus$\backslash$nOphiognomonia clavigigenti-juglandacearum, which has left the remaining$\backslash$nbutternuts isolated and sparsely distributed. To manage the remaining$\backslash$npopulations effectively, information regarding how butternut's$\backslash$npopulation genetic structure is affected by environmental and historical$\backslash$nfactors is needed. In this study, we assessed genetic structure and$\backslash$ndiversity of 161 butternut trees from 19 adjacent watersheds in the$\backslash$nsouthern portion of butternut's range using 12 microsatellite markers.$\backslash$nWe assessed the genetic diversity and genetic differentiation among$\backslash$ntrees grouped at various spatial scales. Our goal was to use historical$\backslash$nabundance and land use data for these watersheds, which are now all a$\backslash$npart of the Great Smoky Mountains National Park (GSMNP), to understand$\backslash$nthe ecological and evolutionary forces that challenge the conservation$\backslash$nand management of butternut. In general, butternuts within the 19$\backslash$nneighboring watersheds were all part of one continuous population, with$\backslash$ngene flow throughout. Significant genetic differentiation was detected$\backslash$nbetween some groups of trees, but the differentiation was quite small$\backslash$nand may not represent an ecologically significant distinction. The mean$\backslash$nheterozygosity in all watersheds remained high, despite extensive$\backslash$nmortality. Overall, genetic diversity and rare alleles were evenly$\backslash$ndistributed across all watersheds, with some variability in$\backslash$nsubpopulations containing butternut-Japanese walnut hybrids (Juglans x$\backslash$nbixbyi or buarts). These results indicate that management of this$\backslash$nspecies should focus on protection from future hybridization with$\backslash$nJapanese walnut, promotion of regeneration, and persistence of all$\backslash$nremaining butternut trees, which still retain high levels of genetic$\backslash$ndiversity.},
	author = {Parks, Amanda and Jenkins, Michael and Ostry, Michael and Zhao, Peng and Woeste, Keith},
	date-added = {2021-07-01 12:00:14 +0200},
	date-modified = {2021-07-01 12:00:16 +0200},
	doi = {10.1007/s11295-014-0702-8},
	file = {:Users/giuliano/Library/Application Support/Mendeley Desktop/Downloaded/Parks et al. - 2014 - Biotic and abiotic factors affecting the genetic structure and diversity of butternut in the southern Appalachian.pdf:pdf},
	issn = {1614-2942},
	journal = {Tree Genetics {\&} Genomes},
	keywords = {Butternut canker disease; Juglans cinerea; Forest},
	number = {3},
	pages = {541--554},
	title = {{Biotic and abiotic factors affecting the genetic structure and diversity of butternut in the southern Appalachian Mountains, USA}},
	volume = {10},
	year = {2014},
	bdsk-url-1 = {https://doi.org/10.1007/s11295-014-0702-8}}

@article{Parola2001,
	abstract = {Ticks are currently considered to be second only to mosquitoes as vectors of human infectious diseases in the world. Each tick species has preferred environmental conditions and biotopes that determine the geographic distribution of the ticks and, consequently, the risk areas for tickborne diseases. This is particularly the case when ticks are vectors and reservoirs of the pathogens. Since the identification of Borrelia burgdorferi as the agent of Lyme disease in 1982, 15 ixodid-borne bacterial pathogens have been described throughout the world, including 8 rickettsiae, 3 ehrlichiae, and 4 species of the Borrelia burgdorferi complex. This article reviews and illustrate various aspects of the biology of ticks and the tickborne bacterial diseases (rickettsioses, ehrlichioses, Lyme disease, relapsing fever borrelioses, tularemia, Q fever), particularly those regarded as emerging diseases. Methods are described for the detection and isolation of bacteria from ticks and advice is given on how tick bites may be prevented and how clinicians should deal with patients who have been bitten by ticks.},
	author = {Parola, P and Raoult, D},
	date-added = {2021-07-01 12:00:14 +0200},
	date-modified = {2021-07-01 12:00:16 +0200},
	doi = {10.1086/319347},
	isbn = {1058-4838 (Print)},
	issn = {1058-4838},
	journal = {Clinical infectious diseases : an official publication of the Infectious Diseases Society of America},
	keywords = {Animals,Bacteria,Bacteria: isolation {\&} purification,Bacteria: pathogenicity,Bacterial Infections,Bacterial Infections: epidemiology,Bacterial Infections: transmission,Communicable Diseases, Emerging,Humans,Life Cycle Stages,Tick-Borne Diseases,Tick-Borne Diseases: diagnosis,Tick-Borne Diseases: epidemiology,Tick-Borne Diseases: microbiology,Tick-Borne Diseases: therapy,Ticks,Ticks: anatomy {\&} histology,Ticks: classification,Ticks: microbiology},
	number = {6},
	pages = {897--928},
	pmid = {11247714},
	title = {{Ticks and tickborne bacterial diseases in humans: an emerging infectious threat.}},
	url = {http://cid.oxfordjournals.org/cgi/content/long/32/6/897},
