	journal = {Proceedings of the Royal Society B: Biological Sciences},
	keywords = {Amphibia,Amphibians,Atelopus zeteki,Bacteria (microorganisms),Batrachochytrium,Batrachochytrium dendrobatidis,Chytrid fungus,Conservation,Disease,Fungi,Microbiome,Panama [Central America],Probiotics,Rana aurantiaca,Skin bacteria,bacterium,disease treatment,fitness,frog,fungal disease,host-pathogen interaction,host-symbiont interaction,metabolite,microbial community,survival},
