	keywords = {Climate change,Crotalus horridus,Disease ecology,Genetic diversity,Genetic variation,Inbreeding,IxE,Microsatellites,Reptiles},
	mendeley-tags = {Genetic variation,IxE,Microsatellites,Reptiles},
