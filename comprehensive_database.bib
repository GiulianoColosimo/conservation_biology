	journal = {International journal of systematic bacteriology},
	keywords = {Amino Acid Sequence,Bacterial Outer Membrane Proteins,Bacterial Outer Membrane Proteins: analysis,Base Sequence,DNA, Bacterial,DNA, Bacterial: chemistry,DNA, Ribosomal,DNA, Ribosomal: chemistry,Phylogeny,Polymerase Chain Reaction,RNA, Ribosomal, 16S,RNA, Ribosomal, 16S: genetics,Rickettsia,Rickettsia: chemistry,Rickettsia: classification},
