	issn = {08888892},
	journal = {Conservation Biology},
	keywords = {Distinct population segment,Evolutionarily significant unit,IUCN Red List,Nomenclature,Taxonomy},
	number = {6},
	pages = {1584--1594},
	pmid = {17181793},
	title = {{Taxonomic considerations in listing subspecies under the U.S. Endangered Species Act}},
	volume = {20},
