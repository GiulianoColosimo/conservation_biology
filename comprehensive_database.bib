	doi = {10.1371/journal.pone.0019667},
	file = {:Users/giuliano/Library/Application Support/Mendeley Desktop/Downloaded/Di Fonzo et al. - 2011 - The population growth consequences of variation in individual heterozygosity.pdf:pdf},
	issn = {19326203},
	journal = {PLoS ONE},
	keywords = {Heterozygosity, Population growth, Fitness},
	number = {5},
	pmid = {21611172},
	title = {{The population growth consequences of variation in individual heterozygosity}},
	volume = {6},
	year = {2011},
	bdsk-url-1 = {https://doi.org/10.1371/journal.pone.0019667}}

