	file = {:Users/giuliano/Library/Application Support/Mendeley Desktop/Downloaded/Richter et al. - 2011 - Conservation Genetics of the Largest Cluster of Federally Threatened Gopher Tortoise (Gopherus polyphemus) Colon.pdf:pdf},
	isbn = {0018-0831},
	issn = {0018-0831},
	journal = {Herpetologica},
	number = {4},
	pages = {406--419},
	title = {{Conservation Genetics of the Largest Cluster of Federally Threatened Gopher Tortoise (Gopherus polyphemus) Colonies with Implications for Species Management}},
	volume = {67},
	year = {2011},
