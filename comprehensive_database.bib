	isbn = {1662-5188 (Electronic)$\backslash$r1662-5188 (Linking)},
	issn = {1662-5188},
	journal = {Frontiers in Computational Neuroscience},
	keywords = {bibliometric analysis,citation,impact factors,impact factors, peer review, productivity, scienti,peer review,productivity,quality versus,scientific output},
	number = {November},
	pages = {1--6},
	pmid = {22102840},
	title = {{Nine Criteria for a Measure of Scientific Output}},
	volume = {5},
	year = {2011},
	bdsk-url-1 = {https://doi.org/10.3389/fncom.2011.00048}}

