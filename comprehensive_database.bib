	keywords = {Cachryx,Ctenosaura,Iguaninae,Phylogeography,Species tree},
	month = {oct},
	pages = {27--39},
	pmid = {28716742},
	title = {{Never judge an iguana by its spines: Systematics of the Yucatan spiny tailed iguana, Ctenosaura defensor (Cope, 1866)}},
	url = {https://linkinghub.elsevier.com/retrieve/pii/S1055790317305201},
