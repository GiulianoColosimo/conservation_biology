@article{Foulley2006,
	abstract = {Assessing allelic richness in a set of populations requires that variations of sample size be taken into account. One way of doing this is to estimate the number of alleles expected in samples of specified size, using the rarefaction method applied in ecology. An alternative method, based on extrapolation, consists of adding to the number of alleles actually seen in a population the expected number of alleles missing, given the number of genes examined in the population and the allelic frequencies observed over the whole set of populations. Heterogeneity of allelic richness across populations and across loci can also be tested in this framework by numerical re-sampling. Both methods provide a measure of ``private'' allelic richness, a useful criterion in genetic diversity preservation, by allowing evaluation of the uniqueness of each population in terms of allele numbers. The two methods are compared on isozyme loci in the argan tree of Morocco and on microsatellite genotypes in the European pig. In both species, allelic richness and gene diversity behave quasi-independently over the populations compared and a higher differentiation is observed in allelic richness compared to gene diversity. In general, the rarefaction technique is sensitive to the sample size of reference and may lack sensitivity to rare alleles when the sample size of reference is small. Extrapolation may thus be recommended especially when the sample sizes of the populations are either low on average or highly unbalanced among populations.},
	author = {Jean-Louis Foulley and Louis Ollivier},
	date-added = {2023-09-19 10:10:33 +0200},
	date-modified = {2023-09-19 10:36:01 +0200},
	doi = {https://doi.org/10.1016/j.livprodsci.2005.10.021},
	issn = {1871-1413},
	journal = {Livestock Science},
	keywords = {Allelic richness, Marker, Private allele, Genetic diversity, Conservation},
	number = {1},
	pages = {150-158},
	title = {Estimating allelic richness and its diversity},
	url = {https://www.sciencedirect.com/science/article/pii/S0301622605003921},
	volume = {101},
	year = {2006},
	bdsk-url-1 = {https://www.sciencedirect.com/science/article/pii/S0301622605003921},
