@article{Stothard1994,
	abstract = {The eubacterial genus Rickettsia belongs to the alpha subgroup of the phylum Proteobacteria. This genus is usually divided into three biotypes on the basis of vector host and antigenic cross-reactivity characteristics. However, the species Rickettsia bellii does not fit into this classification scheme; this organism has characteristics common to both the spotted fever group and the typhus group biotypes and also exhibits some unique features. Sequences of the 16S rRNA and 23S rRNA genes from Rickettsia rickettsii (spotted fever group), Rickettsia prowazekii (typhus group), and R. bellii were studied to determine the position of R. bellii in the rickettsial classification scheme. The 23S rRNA gene sequences described in this paper are the first 23S rRNA sequences reported for any member of the Rickettsiaceae. The 23S rRNA gene contains substantially more phylogenetic information than is contained in the 16S rRNA sequences, and the 23S rRNA gene sequence has diverged about 1.9 times faster in the three Rickettsia species which we studied. Taken together, the molecular data obtained from the two genes indicate that R. bellii is not a member of either the spotted fever group or the typhus group; rather, this organism appears to be the product of a divergence which predates the separation of the genus into the spotted fever group and the typhus group. Consequently, different combinations of the ancestral characteristics retained by R. bellii have been retained in the more derived lineages of the genus.},
	author = {Stothard, D R and Clark, J B and Fuerst, P a},
	date-added = {2021-07-01 12:00:14 +0200},
	date-modified = {2021-07-01 12:00:16 +0200},
	doi = {10.1099/00207713-44-4-798},
	file = {:Users/giuliano/Library/Application Support/Mendeley Desktop/Downloaded/Stothard, Clark, Fuerst - 1994 - Ancestral divergence of Rickettsia bellii from the spotted fever and typhus groups of Rickettsia and an.pdf:pdf},
	isbn = {0020-7713 (Print) 0020-7713 (Linking)},
	issn = {0020-7713},
	journal = {International journal of systematic bacteriology},
	number = {4},
	pages = {798--804},
	pmid = {7981106},
	title = {{Ancestral divergence of Rickettsia bellii from the spotted fever and typhus groups of Rickettsia and antiquity of the genus Rickettsia.}},
	volume = {44},
	year = {1994},
	bdsk-url-1 = {https://doi.org/10.1099/00207713-44-4-798}}

@article{Strobel2016,
	abstract = {Comparative studies of genetic diversity and population structure can shed light on the ecological and$\backslash$r$\backslash$nevolutionary factors that influence host--parasite interactions. Here we examined whether geography, time and$\backslash$r$\backslash$ngenetic variation in Alaskan three-spined stickleback (Gasterosteus aculeatus Linneaus) hosts shape the$\backslash$r$\backslash$npopulation genetic structure of the diphyllobothridean cestode parasite Schistocephalus solidus (Muller, 1776). €$\backslash$r$\backslash$nHost lineages and haplotypes were identified by sequencing the mitochondrial cytochrome b gene, and host$\backslash$r$\backslash$npopulation structure was assessed by Bayesian clustering analysis of allelic variation at 11 microsatellite loci.$\backslash$r$\backslash$nParasite population structure was characterized according to allelic variation at eight microsatellite loci. Mantel$\backslash$r$\backslash$ntests and canonical redundancy analysis were conducted to evaluate the proportion of parasite genetic variation$\backslash$r$\backslash$nattributable to time and geography vs. host lineage, haplotype, and genotypic cluster. Host and parasite$\backslash$r$\backslash$npopulation structure were largely discordant across the study area, probably reflecting differences in gene flow,$\backslash$r$\backslash$nenvironmental influences external to the host, and genomic admixture among host lineages. We found that$\backslash$r$\backslash$ngeography explained the greatest proportion of parasite genetic variation, but that variation also reflects time,$\backslash$r$\backslash$nhost lineage, and host haplotype. Associations with host haplotypes suggest that one parasite genotypic cluster$\backslash$r$\backslash$nexhibits a narrower host range, predominantly infecting the most common host haplotypes, whereas the other$\backslash$r$\backslash$nparasite cluster infects all haplotypes equally, including rare haplotypes. Although experimental infection trials$\backslash$r$\backslash$nmight prove otherwise, distributional differences in hosts preferentially infected by S. solidus could underlie the$\backslash$r$\backslash$nobserved pattern of population structure.},
	author = {Strobel, Hannah M. and Alda, Fernando and Sprehn, C. Grace and Blum, Michael J. and Heins, David C.},
	date-added = {2021-07-01 12:00:14 +0200},
	date-modified = {2021-07-01 12:00:16 +0200},
