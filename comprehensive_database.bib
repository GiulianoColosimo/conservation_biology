	keywords = {Atlantic salmon,Salmo salar,Salmon lice,anti-parasite behaviour,ectoparasite,host--parasite interaction,infestation success},
	month = {jan},
	number = {1},
	pages = {73--80},
	title = {{Behaviour in Atlantic salmon confers protection against an ectoparasite}},
	url = {http://doi.wiley.com/10.1111/jzo.12498},
