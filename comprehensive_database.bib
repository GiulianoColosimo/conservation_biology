	isbn = {0169-5347 (Print)$\backslash$n0169-5347 (Linking)},
	issn = {01695347},
	journal = {Trends in Ecology and Evolution},
	keywords = {Inbreeding avoidance,Inbreeding preference,Inbreeding strategy,Inclusive fitness,Mate choice,Relatedness},
	number = {4},
	pages = {205--211},
	pmid = {23182684},
	publisher = {Elsevier Ltd},
	title = {{Inbreeding avoidance, tolerance, or preference in animals?}},
