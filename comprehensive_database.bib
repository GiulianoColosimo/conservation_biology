	isbn = {0962-1083 (Print)$\backslash$n0962-1083 (Linking)},
	issn = {09621083},
	journal = {Molecular Ecology},
	keywords = {Barriers,Bayesian computations,Capreolus capreolus,Connectivity,Landscape genetics,Population structure},
