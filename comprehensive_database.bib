	isbn = {0014-3820},
	issn = {0014-3820},
	journal = {Evolution; international journal of organic evolution},
	keywords = {Fixed drift load, Genetic drift, Inbreeding load, Purging selection, Inbreeding, Genetic load},
	number = {10},
	pages = {2242--2251},
	pmid = {14628912},
	title = {{Population size and the nature of genetic load in Gentianella germanica.}},
