	doi = {10.1670/09-046.1},
	file = {:Users/giuliano/Library/Application Support/Mendeley Desktop/Downloaded/Fattorini - 2010 - Influence of Recent Geography and Paleogeography on the Structure of Reptile Communities in a Land-Bridge Archipelago.pdf:pdf},
	isbn = {0022-1511},
	issn = {0022-1511},
	journal = {Journal of Herpetology},
	number = {2},
	pages = {242--252},
	title = {{Influence of Recent Geography and Paleogeography on the Structure of Reptile Communities in a Land-Bridge Archipelago}},
	volume = {44},
