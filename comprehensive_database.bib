	issn = {09528369},
	journal = {Journal of Zoology},
	keywords = {Evolution,Populations,Speciation,Hybridization},
	pages = {729--739},
	title = {{Hybridization between the Galapagos land and marine iguana ({\textless}i{\textgreater}Conolophus subcristatus{\textless}/i{\textgreater} and {\textless}i{\textgreater}Amblyrhynchus cristatus{\textless}/i{\textgreater}) on Plaza Sur}},
	volume = {242},
