	isbn = {0962-1083},
	issn = {1365-294X},
	journal = {Molecular Ecology},
	keywords = {Gal{\'a}pagos, Marine iguanas, mtDNA},
	number = {5},
	pages = {437--452},
	title = {{The microevolution of the Gal{\'{a}}pagos marine iguana {\textless}i{\textgreater}Amblyrhynchus cristatus{\textless}/i{\textgreater} assessed by nuclear and mitochondrial genetic analyses}},
	url = {http://onlinelibrary.wiley.com/doi/10.1046/j.1365-294X.1997.00209.x/abstract},
	volume = {6},
	year = {1997},
	bdsk-url-1 = {http://onlinelibrary.wiley.com/doi/10.1046/j.1365-294X.1997.00209.x/abstract},
	bdsk-url-2 = {https://doi.org/10.1046/j.1365-294X.1997.00209.x}}

