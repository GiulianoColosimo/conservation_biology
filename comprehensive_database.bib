	number = {ii},
	pages = {30--34},
	pmid = {343},
	title = {{Ticks {\textless}i{\textgreater}Ixodes uriae{\textless}/i{\textgreater} and the breeding performance of a colonial seabird, king penguin Aptenodytes patagonicus}},
	url = {http://onlinelibrary.wiley.com/doi/10.1034/j.1600-048X.2003.02916.x/full},
