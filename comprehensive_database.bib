	isbn = {1755-098X},
	issn = {1755098X},
	journal = {Molecular Ecology Resources},
	keywords = {Heterozygosity Fitness Correlation,Heterozygosity-heterozygosity correlation,Homozygosity,Inbreeding,Internal relatedness,Multilocus heterozygosity,Software,Standardized heterozygosity},
	mendeley-tags = {Heterozygosity Fitness Correlation,Software},
	number = {4},
