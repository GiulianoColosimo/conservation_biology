	mendeley-tags = {Habitat selection,SSF},
	month = {may},
	number = {5},
	pages = {603--614},
	title = {{Inferring the effects of landscape structure on roe deer ({\textless}i{\textgreater}Capreolus capreolus{\textless}/i{\textgreater}) movements using a step selection function}},
