	issn = {1081-1710},
	journal = {Journal of vector ecology : journal of the Society for Vector Ecology},
	keywords = {amblyomma americanum,and wild turkeys in,areas once cleared for,demographic history,diminished hunting practices,farms and an increase,in numbers of deer,index,is due in part,population expansion,population genetics,response to reforestation and,to resurgence of forested},
