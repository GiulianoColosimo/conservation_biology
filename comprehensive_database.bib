	doi = {10.1046/j.1095-8312.2002.00107.x},
	isbn = {1095-8312},
	issn = {00244066},
	journal = {Biological Journal of the Linnean Society},
	keywords = {Birds,Cospeciation,Ctochrome oxidase I,Ectoparasites,Elongation factor-1$\alpha$,Passerines,Phthiraptera,Systematics},
	number = {2},
	pages = {233--247},
	title = {{The phylogeny of the louse genus {\textless}i{\textgreater}Brueelia{\textless}/i{\textgreater} does not reflect host phylogeny}},
	volume = {77},
	year = {2002},
	bdsk-url-1 = {https://doi.org/10.1046/j.1095-8312.2002.00107.x}}

