	issn = {20457758},
	journal = {Ecology and Evolution},
	keywords = {Baseline levels,ELISA,Glucocorticoids,Hemoparasites,Hepatozoon,Iguanas,Parasitemia,Pink iguana,Wolf volcano},
	mendeley-tags = {Hemoparasites,Iguanas},
