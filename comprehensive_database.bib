	journal = {Journal of Ornithology},
	keywords = {Stable isotopes, Diet analysis},
	number = {2},
	pages = {555--562},
	title = {{Habitat partitioning and molting site fidelity in \emph{Tetrao urogallus cantabricus} revealed through stable isotopes analysis}},
