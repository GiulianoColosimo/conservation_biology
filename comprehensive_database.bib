	keywords = {Bradyrhizobium,Coevolution,Geographic variation,Leguminosae,Mutualism},
	number = {5},
	pages = {549--562},
	pmid = {5551},
	title = {{Geographic structure of lineage associations in a plant-bacterial mutualism}},
	volume = {11},
	year = {1998},
