@article{Knapp2004b,
	abstract = {With the exception of trophic ecology, there are few published detailed accounts on life history and ecological attributes of West Indian boas (genus Epicrates). During an unrelated diurnal study of natal dispersal in hatchlings of the Andros iguana (Cyclura cychlura cychlura), we were able to opportunistically track, record movements, and document diurnal refugia sites for six Bahamian boas (Epicrates striatus fowleri) from Andros Island after they consumed Cyclura neonates fitted with radio transmitters. We report a new prey item for the genus Epicrates, and document a range extension for the Bahamian boa. Our telemetry data suggest that Bahamian boas can move considerably after consuming a meal, and also supports existing published accounts of habitat associations.},
	author = {Knapp, Charles R. and Owens, Audrey K},
	date-added = {2021-07-01 12:00:14 +0200},
	date-modified = {2021-07-01 12:18:42 +0200},
	isbn = {0008-6452},
	journal = {Caribbean Journal of Science},
	keywords = {Epicrates, Bahamas},
