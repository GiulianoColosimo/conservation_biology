@article{Miller2022,
	abstract = {The idea of `key innovations' has long been influential in theoretical and empirical approaches to understanding adaptive diversification. Despite originally revolving around traits inducing major ecological shifts, the key innovation concept itself has evolved, conflating lineage diversification with trait-dependent ecological shifts. In this opinion article we synthesize the history of the term, clarify the relationship between key innovations and adaptive radiation, and propose a return to the original concept of key innovations: the evolution of organismal features which permit a species to occupy a previously inaccessible ecological state. Ultimately, we suggest an integrative approach to studying key innovations, necessitating experimental approaches of form and function, natural history studies of resource use, and phylogenetic comparative perspectives.},
	author = {Aryeh H. Miller and James T. Stroud and Jonathan B. Losos},
