	pmid = {14628950},
	title = {{{\textless}i{\textgreater}Aponomma hydrosauri{\textless}/i{\textgreater}, the reptile-associated tick reservoir of {\textless}i{\textgreater}Rickettsia honei{\textless}/i{\textgreater} on Flinders Island, Australia.}},
	volume = {69},
