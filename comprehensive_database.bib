	isbn = {0098-0331},
	issn = {00980331},
	journal = {Journal of Chemical Ecology},
	keywords = {Chemoreception,Cryptic species,Ethological isolation mechanisms,Lizards,Podarcis,Reptiles,Species recognition},
	number = {7},
