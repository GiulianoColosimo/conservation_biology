	eprint = {1051-0761},
	file = {:Users/giuliano/Library/Application Support/Mendeley Desktop/Downloaded/Palumbi - 2003 - Population genetics, demographic connectivity, and the design of marine reserves.pdf:pdf},
	isbn = {1051-0761},
	issn = {10510761},
	journal = {Ecological Applications},
	keywords = {Connectivity,Dispersal,Isolation by distance,Larvae,Population structure},
	number = {1 SUPPL.},
	pages = {146--158},
	pmid = {1725},
