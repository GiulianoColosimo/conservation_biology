	isbn = {0022-0477},
	issn = {00220477},
	journal = {Journal of Ecology},
	keywords = {Competition,Confounding effect,Logistic regression,Neighbourhood analysis,Population dynamics,Ripley's K function,Spatial pattern,Stand mapping},
	number = {4},
	pages = {676--688},
	pmid = {3500916},
	title = {{Density-dependent effects on tree survival in an old-growth Douglas fir forest}},
