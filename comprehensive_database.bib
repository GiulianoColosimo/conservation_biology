	doi = {10.3354/meps09749},
	file = {:Users/giuliano/Library/Application Support/Mendeley Desktop/Downloaded/McCoy et al. - 2012 - Population genetic structure and colonisation of the western Antarctic Peninsula by the seabird tick iIxodes uriae.pdf:pdf},
	isbn = {0171-8630},
	issn = {01718630},
	journal = {Marine Ecology Progress Series},
	keywords = {Climate change,Host-parasite interactions,Invasion,Pygoscelis,Seabird population dynamics},
	pages = {109--120},
	title = {{Population genetic structure and colonisation of the western Antarctic Peninsula by the seabird tick {\textless}i{\textgreater}Ixodes uriae{\textless}/i{\textgreater}}},
	volume = {459},
