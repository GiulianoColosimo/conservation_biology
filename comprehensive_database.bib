	file = {:Users/giuliano/Library/Application Support/Mendeley Desktop/Downloaded/Meganathan et al. - 2010 - Molecular phylogenetic analyses of genus Crocodylus (Eusuchia, Crocodylia, Crocodylidae) and the taxonomic po.pdf:pdf},
	isbn = {1055-7903},
	issn = {10557903},
	journal = {Molecular Phylogenetics and Evolution},
	keywords = {Crocodylus,Crocodylus porosus,Genetic distances,Intrageneric relationship,Mitochondrial genome,Phylogeny},
