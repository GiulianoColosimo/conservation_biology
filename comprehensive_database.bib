	file = {:Users/giuliano/Library/Application Support/Mendeley Desktop/Downloaded/Schulte, Valladares, Larson - 2003 - Phylogenetic Relationships Within Iguanidae Inferred Using Molecular and Morphological Data and a P.pdf:pdf},
	isbn = {0018-0831},
	issn = {0018-0831},
	journal = {Herpetologica},
	keywords = {iguania,iguanidae,metataxon,phylogenetic taxonomy,polychrotinae,squamata,topology},
