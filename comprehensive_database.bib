	file = {:Users/giuliano/Library/Application Support/Mendeley Desktop/Downloaded/Lee, Chow - 2013 - The causal meaning of Fisher's average effect.pdf:pdf},
	isbn = {1469-5073 (Electronic)},
	issn = {1469-5073},
	journal = {Genet Res (Camb)},
	number = {2-3},
	pages = {89--109},
	pmid = {23938113},
	title = {{The causal meaning of Fisher's average effect}},
