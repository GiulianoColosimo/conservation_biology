	file = {:Users/giuliano/Library/Application Support/Mendeley Desktop/Downloaded/Aylagas, Borja, Rodr{\'{i}}guez-Ezpeleta - 2014 - Environmental status assessment using DNA metabarcoding Towards a genetics based marine bio.pdf:pdf},
	isbn = {10.1371/journal.pone.0090529},
	issn = {19326203},
	journal = {PLoS ONE},
	keywords = {Barcoding,mtDNA},
	mendeley-tags = {Barcoding,mtDNA},
	number = {3},
	pmid = {24603433},
	title = {{Environmental status assessment using DNA metabarcoding: Towards a genetics based marine biotic index (gAMBI)}},
