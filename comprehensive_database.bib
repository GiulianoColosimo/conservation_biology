	issn = {0098-0331},
	journal = {Journal of Chemical Ecology},
	keywords = {Behavior cycle,Iguania,Phrynosomatidae,Reptilia,Sceloporus graciosus,Scent mark},
	month = {apr},
	number = {4},
	pages = {881--893},
	title = {{Individual, Sexual, Seasonal, and Temporal Variation in the Amount of Sagebrush Lizard Scent Marks}},
	url = {http://link.springer.com/10.1007/s10886-006-9029-8},
	volume = {32},
