@article{Wilson1995,
	abstract = {Analysis of magnetic anomaly profiles collected nearly parallel to tectonic flow lines allows detailed interpretation of the complicated tectonic history of the Cocos-Nazca spreading center. Forward models of the magnetic anomalies accounting for spreading rate variations, ridge jumps, asymmetric spreading, magnetization intensity variations, and bathymetry show excellent agreement with observed anomalies. Spreading rates can be constrained to a common finite rotation history through anomaly 4A with three changes in rates. Rift propagation has played a dominant role in the continuous reorganization of the geometry of the ridge axis. Propagation has been predominantly away from the hotspot, with humps predominantly southward. Propagation rates have ranged from 30 to 120 mm/yr, commonly near 70 mm/yr. Several generalizations are drawn about along-axis variations in magnetization intensities since 8 Ma. -from Authors},
	author = {Wilson, D. S. and Hey, R. N.},
	date-added = {2021-07-01 12:00:14 +0200},
