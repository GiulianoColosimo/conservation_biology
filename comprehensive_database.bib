	file = {:Users/giuliano/Library/Application Support/Mendeley Desktop/Downloaded/Grativol, Ballou, Fleischer - 2001 - Microsatellite variation within and among recently fragmented populations of the golden lion tamari.pdf:pdf},
	isbn = {1566-0621},
