	isbn = {1543-592X},
	issn = {1543-592X},
	journal = {Annual Review of Ecology, Evolution, and Systematics},
	keywords = {behavioral},
	number = {1},
	pages = {23--46},
	title = {{Life-History Evolution in Reptiles}},
	volume = {36},
	year = {2005},
	bdsk-url-1 = {https://doi.org/10.1146/annurev.ecolsys.36.102003.152631}}

