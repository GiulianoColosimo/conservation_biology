	journal = {Journal of Animal Ecology},
	keywords = {Animal movements, Radiotracking, Site fidelity, Space use, Telemetry, Home range},
	number = {6},
	pages = {1393-1405},
	title = {Effects of sampling regime on the mean and variance of home range size estimates},
	url = {https://besjournals.onlinelibrary.wiley.com/doi/abs/10.1111/j.1365-2656.2006.01164.x},
	volume = {75},
	year = {2006},
