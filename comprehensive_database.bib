	issn = {02698463},
	journal = {Functional Ecology},
	keywords = {Lacertidae,adaptive evolution,chemical communication,epidermal gland secretions,macroevolution,pheromones,phylogenetic comparative methods,signal efficacy},
	month = {feb},
	number = {2},
	pages = {566--580},
	title = {{Environmental conditions shape the chemical signal design of lizards}},
	url = {http://doi.wiley.com/10.1111/1365-2435.12984},
