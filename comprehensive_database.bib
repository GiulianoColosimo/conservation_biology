	issn = {0022-1511},
	journal = {Journal of Herpetology},
	number = {2},
	pages = {308--313},
	title = {{Fine Scale Genetic Structure in a Population of the Prehensile Tailed Skink, Corucia zebrata}},
	volume = {47},
