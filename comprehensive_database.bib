	date-modified = {2021-07-01 12:00:15 +0200},
	doi = {10.1655/08-036R1.1},
	file = {:Users/giuliano/Library/Application Support/Mendeley Desktop/Downloaded/Kacoliris, Berkunsky, Williams - 2009 - Methods for Assessing Population Size in Sand Dune Lizards (Liolaemus Multimaculatus).pdf:pdf},
	isbn = {0018-0831},
	issn = {0018-0831},
	journal = {Herpetologica},
	number = {2},
	pages = {219--226},
	title = {{Methods for Assessing Population Size in Sand Dune Lizards (Liolaemus Multimaculatus)}},
	volume = {65},
