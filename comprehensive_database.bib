	journal = {Journal of zoo and wildlife medicine : official publication of the American Association of Zoo Veterinarians},
	keywords = {Animals,Eye,Eye: anatomy {\&} histology,Female,Intraocular Pressure,Intraocular Pressure: physiology,Lizards,Lizards: physiology,Male,Ocular Physiological Phenomena,Reference Values,Tears,Tears: physiology,Tonometry, Ocular,Tonometry, Ocular: veterinary},
