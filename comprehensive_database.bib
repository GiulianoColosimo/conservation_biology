	file = {:Users/giuliano/Library/Application Support/Mendeley Desktop/Downloaded/Rijks et al. - 2008 - Heterozygosity and lungworm burden in harbour seals (iPhoca vitulinai).pdf:pdf},
	isbn = {0018-067X},
	issn = {0018-067X},
	journal = {Heredity},
	keywords = {Heterozygosity Fitness Correlation,Microsatellites,circumlitus,fitness correlation,harbour seal,heterozygosity,hfc,lungworm burden,otostrongylus,parafilaroides gymnurus,phoca vitulina},
	mendeley-tags = {Heterozygosity Fitness Correlation,Microsatellites},
