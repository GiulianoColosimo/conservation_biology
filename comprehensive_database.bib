	doi = {10.1111/j.1365-294X.2006.03017.x},
	issn = {09621083},
	journal = {Molecular Ecology},
	keywords = {Assignment tests,Hatchery releases,Microsatellites,Parental identification,Salmonids},
	month = {aug},
	number = {11},
	pages = {3157--3173},
	pmid = {16968262},
	title = {{An empirical verification of population assignment methods by marking and parentage data: hatchery and wild steelhead ({\textless}i{\textgreater}Oncorhynchus mykiss{\textless}/i{\textgreater}) in Forks Creek, Washington, USA}},
	url = {http://doi.wiley.com/10.1111/j.1365-294X.2006.03017.x},
	volume = {15},
	year = {2006},
	bdsk-url-1 = {http://doi.wiley.com/10.1111/j.1365-294X.2006.03017.x},
	bdsk-url-2 = {https://doi.org/10.1111/j.1365-294X.2006.03017.x}}

@article{Hayashi1985,
	abstract = {Recombination of mammalian mitochondrial DNA (mtDNA) was examined using mouse X rat somatic cell hybrid clones and rat cybrid clones. The mouse X rat hybrids were isolated by fusion of chloramphenicol-sensitive (CAPs) mouse and CAP-resistant (CAPr) rat cells. The rat cybrids were isolated by fusion of rat cells with type B mtDNA and enucleated cells with type A mtDNA. Genetic and physical analyses showed that the mtDNAs of the hybrids and cybrids were simple mixtures of the two parental mtDNAs except in the following two cases: One was subclone H29 of mouse X rat hybrids, which was CAPr even though mtDNA from the CAPs mouse parent was predominantly retained. The other was rat cybrid subclones, Y1224 and -61, which showed specific loss of one Hinf I fragment of type B mtDNA, B10. These observations suggest that, in contrast to the case with plant mtDNA, recombination of mammalian mtDNA occurs rarely, if at all. {\textcopyright} 1985.},
	author = {Hayashi, Jun Ichi and Tagashira, Yusaku and Yoshida, Michihiro C.},
	date-added = {2021-07-01 12:00:14 +0200},
