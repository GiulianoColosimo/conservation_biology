	issn = {0090-3558},
	journal = {Journal of Wildlife Diseases},
	keywords = {R,R-packages,bat,color quantification,destructans,fungal infection,myotis lucifugus,pseudogymnoascus,r package,uv transillumination photographs,white-nose syndrome},
