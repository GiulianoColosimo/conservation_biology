	number = {3},
	pages = {257--270},
	title = {{Corrected western Atlantic sea-level curve for the last 11,000 years based on calibrated {\textless}sup{\textgreater}14{\textless}/sup{\textgreater}C dates from Acropora palmata framework and intertidal mangrove peat}},
