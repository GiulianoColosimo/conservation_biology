	journal = {Journal of Biogeography},
	keywords = {Butterfly genitalia,Genitalia,Geometric morphometrics,Glaciations,Hybridization,Introgression,Pyronia,Refugia,Rhopalocera,Western Mediterranean},
	mendeley-tags = {Butterfly genitalia},
	number = {5},
