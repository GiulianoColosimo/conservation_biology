	isbn = {1055-7903},
	issn = {10557903},
	journal = {Molecular Phylogenetics and Evolution},
	keywords = {Annotation,Genome,Metazoa,Mitochondria,Server,Software},
	mendeley-tags = {Genome,Software},
	number = {2},
	pages = {313--319},
	pmid = {22982435},
	publisher = {Elsevier Inc.},
	title = {{MITOS: Improved de novo metazoan mitochondrial genome annotation}},
