	journal = {Animal Conservation},
	number = {3},
	pages = {223-233},
	title = {Assessing population structure at high levels of differentiation: microsatellite comparisons of bighorn sheep and large carnivores},
	url = {https://zslpublications.onlinelibrary.wiley.com/doi/abs/10.1111/j.1469-1795.1999.tb00068.x},
