	isbn = {1558-5646},
	issn = {0014-3820},
	journal = {Evolution; international journal of organic evolution},
	keywords = {Drift, Genetic variation, Heritability, Heterozygosity, Inbreeding},
	number = {10},
	pages = {2159--2169},
	pmid = {16405160},
	title = {{Modeling the adaptive potential of isolated populations: experimental simulations using Drosophila.}},
	volume = {59},
	year = {2005},
	bdsk-url-1 = {https://doi.org/10.1111/j.0014-3820.2005.tb00925.x}}

