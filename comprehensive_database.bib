	doi = {10.2307/2640721},
	isbn = {0014-3820},
	issn = {00143820},
	journal = {Evolution},
	keywords = {Author Keywords:,DROSOPHILA-MELANOGASTER LARVAE,KeyWords Plus:,POPULATION-STRUCTU,Population structure,cytochrome oxidase I and II,geographic structure,mtDNA},
	mendeley-tags = {Population structure,mtDNA},
	number = {3},
	pages = {818--825},
	title = {{Comparative geographic structures of two parasitoid-host interactions}},
	volume = {53},
