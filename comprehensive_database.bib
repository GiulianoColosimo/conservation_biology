	isbn = {1435-9456 (Electronic)$\backslash$n1435-9448 (Linking)},
	issn = {14359448},
	journal = {Animal Cognition},
	keywords = {Domestic cat,Felis catus,Human-cat interaction,Vocal recognition},
	number = {4},
	pages = {685--690},
	pmid = {23525707},
	title = {{Vocal recognition of owners by domestic cats (Felis catus)}},
