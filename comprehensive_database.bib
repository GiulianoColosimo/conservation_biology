	keywords = {Broken stick model,GPS telemetry,Geographic Information System GIS,Habitat selection,Individual modelling,Remote sensing,Resource Selection Function RSF,Resource Selection Probability Function RSPF,SSF,State-space model,Step Selection Function SSF},
	mendeley-tags = {Habitat selection,SSF},
