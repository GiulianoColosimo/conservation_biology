	isbn = {1365-2540 (Electronic)$\backslash$r0018-067X (Linking)},
	issn = {1365-2540},
	journal = {Heredity (Edinb)},
	keywords = {Inbreeding depression, Environment},
	mendeley-tags = {Inbreeding,IxE},
	number = {1},
	pages = {50--60},
	pmid = {21224873},
	title = {{The effects of age and environment on the expression of inbreeding depression in Eucalyptus globulus}},
	url = {http://www.ncbi.nlm.nih.gov/pubmed/21224873},
	volume = {107},
