	issn = {03781119},
	journal = {Gene},
	keywords = {Codon usage,Crocodylus johnstoni,Crocodylus moreletii,Crocodylus palustris,Mitochondrial genome,Phylogenetic relationships},
	number = {1-2},
	pages = {35--41},
	pmid = {21310220},
	publisher = {Elsevier B.V.},
	title = {{Complete mitochondrial genome sequences of three Crocodylus species and their comparison within the Order Crocodylia}},
