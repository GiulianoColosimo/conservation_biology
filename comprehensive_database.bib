	doi = {10.1073/pnas.0806339106},
	file = {:Users/giuliano/Library/Application Support/Mendeley Desktop/Downloaded/Gentile et al. - 2009 - An overlooked pink species of land iguana in the Galapagos.pdf:pdf},
	isbn = {1091-6490 (Electronic)$\backslash$n0027-8424 (Linking)},
	issn = {0027-8424},
	journal = {Proceedings of the National Academy of Sciences},
	keywords = {Conolophus marthae, Gal{\'a}pagos},
	number = {2},
	pages = {507--511},
	pmid = {19124773},
	title = {{An overlooked pink species of land iguana in the Gal{\'{a}}pagos}},
	url = {http://www.pnas.org/cgi/doi/10.1073/pnas.0806339106},
	volume = {106},
