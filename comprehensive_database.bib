	isbn = {1055-7903},
	issn = {10557903},
	journal = {Molecular Phylogenetics and Evolution},
	keywords = {18S, 28S, Amblyomma, Genome, Ixodida, Mitochondrial genomes, Phylogeny, Polyphyly, Ticks},
	mendeley-tags = {Genome,Phylogeny,Ticks},
	number = {1},
	pages = {45--55},
