	journal = {BMC Biology},
	keywords = {Bayesian analyses, Random clock, Local clocks},
	number = {1},
	pages = {114},
	pmid = {20807414},
	title = {{Bayesian random local clocks, or one rate to rule them all}},
	url = {http://bmcbiol.biomedcentral.com/articles/10.1186/1741-7007-8-114},
