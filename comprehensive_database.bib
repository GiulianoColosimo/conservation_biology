	journal = {Herpetologica},
	keywords = {Cyclura pinguis, Invasive species, Introductions, Competition},
	number = {1},
	pages = {7--17},
	publisher = {[Herpetologists' League, Allen Press]},
	title = {{Effect of Introduced Ungulates on Density, Dietary Preferences, Home Range, and Physical Condition of the Iguana (\emph{Cyclura pinguis}) on Anegada}},
	url = {http://www.jstor.org/stable/3893064},
	urldate = {2023-07-06},
