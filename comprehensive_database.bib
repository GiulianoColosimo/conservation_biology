	journal = {Molecular Ecology Resources},
	keywords = {Genetic markers,Maximum likelihood,Parentage,Sibship,Simulations},
	pages = {734--739},
	pmid = {23615269},
	title = {{A simulation module in the computer program colony for sibship and parentage analysis}},
	volume = {13},
