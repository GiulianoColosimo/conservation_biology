	journal = {Computers & Geosciences},
	keywords = {Kriging, Cokriging, Linear model of coregionalisation, Open source software, S language, Stochastic simulation, gstat},
	number = {7},
	pages = {683-691},
	title = {Multivariable geostatistics in S: the gstat package},
	url = {https://www.sciencedirect.com/science/article/pii/S0098300404000676},
	volume = {30},
	year = {2004},
	bdsk-url-1 = {https://www.sciencedirect.com/science/article/pii/S0098300404000676},
