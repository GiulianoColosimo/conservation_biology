	isbn = {1365-294X},
	issn = {09621083},
	journal = {Molecular Ecology},
	keywords = {Suricata suricatta,emergence mass,growth rate,inbreeding,inbreeding depression,juvenile survival},
	number = {11},
	pages = {2788--2804},
	pmid = {22497583},
	title = {{Inbreeding and inbreeding depression of early life traits in a cooperative mammal}},
	volume = {21},
