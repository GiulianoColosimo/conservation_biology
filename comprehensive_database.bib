	keywords = {Animals,Computer Simulation,Drosophila melanogaster,Drosophila melanogaster: genetics,Epistasis, Genetic,Epistasis, Genetic: physiology,Genetic Drift,Genetic Fitness,Genetic Fitness: physiology,Genetics, Population,Inbreeding,Linkage Disequilibrium,Models, Genetic,Mutation,Mutation: physiology,Selection, Genetic},
	number = {1},
