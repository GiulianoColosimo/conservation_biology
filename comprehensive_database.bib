	keywords = {Methods,Microsatellites},
	mendeley-tags = {Methods,Microsatellites},
	number = {2},
	pages = {839--852},
	pmid = {11606557},
	title = {{Dynamics of microsatellite divergence under stepwise mutation and proportional slippage/point mutation models}},
	volume = {159},
