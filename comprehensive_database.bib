	journal = {Restoration Ecology},
	keywords = {Cyclura pinguis, Iguana pinguis},
	number = {1},
	pages = {43-50},
	title = {{Status of a Relocated Population of Endangered \emph{Iguana pinguis} on Guana Island, British Virgin Islands}},
	url = {https://onlinelibrary.wiley.com/doi/abs/10.1111/j.1526-100X.1994.tb00040.x},
	volume = {2},
	year = {1994},
	bdsk-url-1 = {https://onlinelibrary.wiley.com/doi/abs/10.1111/j.1526-100X.1994.tb00040.x},
