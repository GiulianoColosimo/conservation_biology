	issn = {0024-4082},
	journal = {Zoological Journal of the Linnean Society},
	keywords = {Hybridization, Hybrid zones, Lizards, Population genetics},
	month = {03},
	number = {2},
	pages = {757-769},
	title = {{Hybridization and hybrid zone stability between two lizards explained by population genetics and niche quantification}},
	url = {https://doi.org/10.1093/zoolinnean/zlaa018},
	volume = {190},
	year = {2020},
