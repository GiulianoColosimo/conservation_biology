	isbn = {0008-4301},
	issn = {0008-4301},
	journal = {Canadian Journal of Zoology-Revue Canadienne De Zoologie},
	keywords = {Anolis carolinensis,Evolutionary significance,Life history traits,Lizard,Phenotypic plasticity},
