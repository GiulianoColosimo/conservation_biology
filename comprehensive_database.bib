	doi = {https://doi.org/10.1111/mms.12726},
	journal = {Marine Mammal Science},
	keywords = {Isotopic niche, Stable isotopes, Trophic niche},
	number = {1},
	pages = {111-126},
	title = {Niche overlap and diet composition of three sympatric coastal dolphin species in the southwest Atlantic Ocean},
	url = {https://onlinelibrary.wiley.com/doi/abs/10.1111/mms.12726},
	volume = {37},
	year = {2021},
	bdsk-url-1 = {https://onlinelibrary.wiley.com/doi/abs/10.1111/mms.12726},
	bdsk-url-2 = {https://doi.org/10.1111/mms.12726}}

