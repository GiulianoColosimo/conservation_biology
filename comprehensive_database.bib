	doi = {10.4319/lo.1994.39.7.1517},
	file = {:Users/giuliano/Library/Application Support/Mendeley Desktop/Downloaded/Anderson, Benke - 1994 - Growth and reproduction of the cladoceran Ceriodaphnia dubia from a forested floodplain swamp.pdf:pdf},
	isbn = {0024-3590},
	issn = {00243590},
	journal = {Limnology and Oceanography},
	keywords = {Growth rate},
	mendeley-tags = {Growth rate},
	number = {7},
	pages = {1517--1527},
	title = {{Growth and reproduction of the cladoceran Ceriodaphnia dubia from a forested floodplain swamp}},
	volume = {39},
