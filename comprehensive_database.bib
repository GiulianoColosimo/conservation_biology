	doi = {10.1002/ece3.3261},
	file = {:Users/giuliano/Dropbox/manuscripts/literature/PDFs/de Magalh{\~{a}}es{\_}Evolutionarily significant units of the critically endangered leaf frog{\_}2017.pdf:pdf},
	issn = {20457758},
	journal = {Ecology and Evolution},
	keywords = {Anura,Approximate Bayesian computation,Campos rupestres,Conservation genetics,ESU,Ecological niche modeling,Niche overlap,Protected areas,Statistical phylogeography},
	mendeley-tags = {Anura,Conservation genetics,ESU,Protected areas},
	month = {nov},
	number = {21},
	pages = {8812--8828},
