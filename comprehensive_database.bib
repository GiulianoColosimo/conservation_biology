@article{Koehn1976,
	abstract = {Approximately 25,000 individuals from 150 samples of the Blue Mussel,$\backslash$nMytilus edulis, collected from Virginia to Iceland were studied at$\backslash$nsix loci. Characterization of both macro- and microgeographic variation$\backslash$nwas done in order to estimate the relative importance of migration$\backslash$nand selection to genetic differentiation among populations. Some$\backslash$nloci exhibited homogeneity in allele frequencies over very large$\backslash$ngeographic distances, while others exhibited differences over large,$\backslash$nmoderate, and very small distances. Frequencies of alleles at the$\backslash$nAP locus were uniform throughout the sampled range, with the exception$\backslash$nof northern Nova Scotia, Newfoundland and Iceland. At the LAP locus$\backslash$nthe commonest allele (0.55-0.58) was uniform from Virginia to southern$\backslash$nCape Cod, excluding Long Island Sound. Within Long Island Sound,$\backslash$nthe frequency of this allele decreased along the Connecticut shore$\backslash$nwith distance from the Atlantic Ocean reaching a minimum value of$\backslash$n0.10 at New York City. North Shore of Long Island samples were all$\backslash$nless than 0.20, even within several hundred yards of the Atlantic$\backslash$nOcean. The variation in frequency is consistent with selection against$\backslash$ncertain genotypes and immigrants transported via known circulation$\backslash$npatterns into Long Island Sound. A similar sharp decrease of the$\backslash$nsame magnitude occurred north of Cape Cod. The GPI locus exhibited$\backslash$nsignificant correlation between allele frequencies and the latitude$\backslash$nfrom which samples were taken. There were no significant deviations$\backslash$nin observed zygotic frequencies from those expected at Hardy-Weinberg$\backslash$nequilibrium at the GPI locus. However, significant deviations were$\backslash$nobserved at both the AP and LAP loci in samples taken from geographic$\backslash$nareas adjacent to major regions of significantly different genetic$\backslash$ncomposition. These significant departures from Hardy-Weinberg, in$\backslash$nthe form of deficiencies of heterozygotes, are attributed at least$\backslash$npartially to the Wahlund effect. Migration does not appear to be$\backslash$na significant factor in the maintenance of the observed patterns$\backslash$nof macrogeographic variation. Attempts to find significant correlations$\backslash$nbetween magnitudes of spatial heterogeneity and genetic composition$\backslash$nand magnitudes of heterozygote deficiency of individual loci were$\backslash$nunsuccessful. Likewise, loci exhibiting significant geographic variation$\backslash$nin genetic composition could not be shown to exhbiit significantly$\backslash$ngreater deficiencies of heterozygotes. Dilocus genotypic compositions$\backslash$ndid not provide additional support for a major role of migration.$\backslash$nPopulations exposed to reductions from normal salinity, exhibit significantly$\backslash$ndifferent genetic composition, especially at the LAP locus. Multiple$\backslash$nsamples from estuaries, exhibited increased among-sample variance$\backslash$nof allele frequencies at the AP, LAP, and GPI loci. Microgeographic$\backslash$ndifferentiation was most dramatic at the LAP locus. Without exception,$\backslash$npopulations exposed to reduced salinity exhibited a significant reduction$\backslash$nin the frequency of the allele most common in marine coastal populations$\backslash$nand a concomitant increase in two alternate alleles. Significant$\backslash$nsize-dependent variation occurred in the frequency of LAP alleles$\backslash$nin some samples. Sharp spatio-temporal differences in allele frequencies$\backslash$noccurred whenever there were (1) intermediate input frequency due$\backslash$nto mixing, and (2) spatial variation in salinity. The smallest individuals$\backslash$nexhibited intermediate frequencies. Subsequent size-dependent changes$\backslash$nin allele frequency were associated with size-dependent changes in$\backslash$nheterozygosity. The direction of change in heterozygosity was dependent$\backslash$nupon the direction of change in allele frequency. These results demonstrate$\backslash$nthat despite the extended larval dispersal period of three to seven$\backslash$nweeks in this species, significant spatio-temporal variation exists$\backslash$nin genetic composition. This variation seems most likely to be determined$\backslash$nby spatial heterogeneity of the environment, possibly interactions$\backslash$nof temperature and salinity. Although significant mortality occurs$\backslash$nin the larval population, considerable differential mortality occurs$\backslash$nin adults.},
	author = {Koehn, R. K. and Milkman, R. and Mitton, J. B.},
	date-added = {2021-07-01 12:00:14 +0200},
	date-modified = {2021-07-01 12:00:15 +0200},
	isbn = {0014-3820},
	issn = {00143820},
	journal = {Evolution},
	number = {1},
	pages = {2--32},
	title = {{Population genetics of marine pelecypods. IV. Selection, Migration and Genetic Differentiation in the Blue Mussel Mytilus edulis}},
	volume = {30},
	year = {1976}}

@article{Koehn1982,
	abstract = {A positive correlation between the degree of individual heterozygosity and growth rate was reported for several species, including the American oyster, C. virginica. More heterozygous individuals of this species exhibit lower .ovrhdot.VO2 O2 consumption rate. When considered within the balanced energy equation, this genetic difference in .ovrhdot.VO2 provides an energetic explanation for higher growth rate of more heterozygous individuals. Following exposure to increased temperature and reduced salinity, the same relationship exists between heterozygosity and .ovrhdot.VO2, but .ovrhdot.VO2 increases with a Q10 of 2.9 in all heterozygosity classes. This results in a metabolic cost 2 times greater for the most homozygous individuals relative to the most heterozygous individuals.},
	author = {Koehn, R. K. and Shumway, S E},
	date-added = {2021-07-01 12:00:14 +0200},
	date-modified = {2021-07-01 12:16:43 +0200},
	isbn = {0165-859X},
	issn = {0165859X},
	journal = {Marine BiolLetters},
	keywords = {Growth rate},
	number = {1},
	pages = {35--42},
	title = {{A Genetic Physiological Explanation for Differential Growth Rate among Individuals of the American Oyster Crassostrea-Virginica}},
	volume = {3},
	year = {1982}}

@article{Kokko2006,
	abstract = {Avoidance of incestuous matings is widely reported across many animal taxa, and the adaptive value of such behavior is explained through inbreeding depression. However, an old and somewhat neglected theoretical result predicts that inbred matings offer another, positive effect on the inclusive fitness of parents: an individual who mates with a relative will help that relative to spread genes identical by descent. This benefit can be substantial, if the additional mating achieved by the relative does not harm his mating success otherwise, and in the context of selfing in plants the phenomenon is well known. Here, we develop a model that derives expected values of inbreeding tolerance, that is, the magnitude of inbreeding depression that is required to make individuals avoid inbreeding, for different animal life histories and parental investment patterns. We also distinguish between simultaneous and sequential mate choice, and show that inbreeding tolerance should often be remarkably high in the latter scenario in particular, although egalitarian parental care will lead to lower tolerance. There is a mismatch between theory and data: the almost complete lack of cases where individuals prefer to mate incestuously is at odds with a large overlap between the predicted range of inbreeding tolerance and estimates of inbreeding depression found in nature. We discuss four different solutions to this enigma, and suggest that inbreeding tolerance, where it is found, should not always be attributed to a simple constraint that has prevented finding any other mate.},
	author = {Kokko, Hanna and Ots, Indrek},
	date-added = {2021-07-01 12:00:14 +0200},
	date-modified = {2021-07-01 14:50:20 +0200},
	doi = {10.1111/j.0014-3820.2006.tb01128.x},
	file = {:Users/giuliano/Library/Application Support/Mendeley Desktop/Downloaded/Kokko, Ots - 2006 - When not to avoid inbreeding.pdf:pdf},
	isbn = {0014-3820},
	issn = {0014-3820},
	journal = {Evolution; international journal of organic evolution},
	keywords = {Inbreeding, Incest avoidance ,Relatedness, Sequential mate choice, Sex roles, Sexual conflict},
	number = {3},
	pages = {467--475},
	pmid = {16637492},
	title = {{When not to avoid inbreeding.}},
	volume = {60},
	year = {2006},
	bdsk-url-1 = {https://doi.org/10.1111/j.0014-3820.2006.tb01128.x}}

@article{Kolbe2004,
	abstract = {A genetic paradox exists in invasion biology: how do introduced populations, whose genetic variation has probably been depleted by population bottlenecks, persist and adapt to new conditions? Lessons from conservation genetics show that reduced genetic variation due to genetic drift and founder effects limits the ability of a population to adapt, and small population size increases the risk of extinction. Nonetheless, many introduced species experiencing these same conditions during initial introductions persist, expand their ranges, evolve rapidly and become invasive. To address this issue, we studied the brown anole, a worldwide invasive lizard. Genetic analyses indicate that at least eight introductions have occurred in Florida from across this lizard's native range, blending genetic variation from different geographic source populations and producing populations that contain substantially more, not less, genetic variation than native populations. Moreover, recently introduced brown anole populations around the world originate from Florida, and some have maintained these elevated levels of genetic variation. Here we show that one key to invasion success may be the occurrence of multiple introductions that transform among-population variation in native ranges to within-population variation in introduced areas. Furthermore, these genetically variable populations may be particularly potent sources for introductions elsewhere. The growing problem of invasive species introductions brings considerable economic and biological costs. If these costs are to be mitigated, a greater understanding of the causes, progression and consequences of biological invasions is needed.},
	author = {Kolbe, Jason J and Glor, Richard E and {Rodr{\'{i}}guez Schettino}, Lourdes and Lara, Ada Chamizo and Larson, Allan and Losos, Jonathan B},
	date-added = {2021-07-01 12:00:14 +0200},
	date-modified = {2021-07-01 12:00:15 +0200},
	doi = {10.1038/nature02807},
	isbn = {0028-0836},
	issn = {1476-4687},
	journal = {Nature},
	keywords = {Animal Migration,Animals,Bayes Theorem,Body Constitution,Caribbean Region,Cuba,DNA,Environment,Florida,Founder Effect,Genetic Variation,Genetic Variation: genetics,Geography,Haplotypes,Haplotypes: genetics,Hawaii,Lizards,Lizards: genetics,Lizards: physiology,Mitochondrial,Mitochondrial: genetics,Phylogeny,Population Dynamics,Taiwan},
	number = {7005},
	pages = {177--81},
	pmid = {15356629},
	title = {{Genetic variation increases during biological invasion by a Cuban lizard.}},
	url = {http://dx.doi.org/10.1038/nature02807},
	volume = {431},
	year = {2004},
	bdsk-url-1 = {http://dx.doi.org/10.1038/nature02807}}

@misc{Kong2016,
	abstract = {Median-joining (MJ) was proposed as a method for phylogeographical analysis and is enjoying increasing popularity. Herein, we evaluate the efficacy of the approach as originally intended. We show that median-joining networks (MJNs) are theoretically untenable for evolutionary inference, and that confusion has afflicted their use for over 15 years. The approach has two obvious shortcomings: its reliance on distance-based phenetics (overall similarity instead of character transformations) and the lack of rooting (no direction or history). Given that evolution involves both change and time, and the absence of rooting removes time (ancestor--descendant relationships) from the equation, the approach cannot yield defensible evolutionary interpretations. We also examine the impact of MJ analyses on evolutionary biology via an analysis of citations and conclude that the spread of MJNs through the literature is difficult to explain, especially given the availability of character-based analyses.},
	author = {Kong, Sungsik and S{\'{a}}nchez-Pacheco, Santiago J. and Murphy, Robert W.},
	booktitle = {Cladistics},
	date-added = {2021-07-01 12:00:14 +0200},
	date-modified = {2021-07-01 12:00:15 +0200},
	doi = {10.1111/cla.12147},
	issn = {10960031},
	title = {{On the use of median-joining networks in evolutionary biology}},
	year = {2016},
	bdsk-url-1 = {https://doi.org/10.1111/cla.12147}}

@article{Kong2011,
	abstract = {Btrim is a fast and lightweight software to trim adapters and low quality regions in reads from ultra high-throughput next-generation sequencing machines. It also can reliably identify barcodes and assign the reads to the original samples. Based on a modified Myers's bit-vector dynamic programming algorithm, Btrim can handle indels in adapters and barcodes. It removes low quality regions and trims off adapters at both or either end of the reads. A typical trimming of 30. M reads with two sets of adapter pairs can be done in about a minute with a small memory footprint. Btrim is a versatile stand-alone tool that can be used as the first step in virtually all next-generation sequence analysis pipelines. The program is available at http://graphics.med.yale.edu/trim/. ?? 2011 Elsevier Inc.},
