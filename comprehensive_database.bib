	doi = {10.1046/j.1523-1739.1996.10010085.x},
	file = {:Users/giuliano/Library/Application Support/Mendeley Desktop/Downloaded/Legge et al. - 1996 - Genetic Criteria for Establishing Evolutionarily Significant Units in Cryan's Buckmoth.pdf:pdf},
	isbn = {0888-8892},
	issn = {08888892},
	journal = {Conservation Biology},
	number = {1},
	pages = {85--98},
	title = {{Genetic Criteria for Establishing Evolutionarily Significant Units in Cryan's Buckmoth}},
	url = {http://www.blackwell-synergy.com/doi/abs/10.1046/j.1523-1739.1996.10010085.x},
	volume = {10},
