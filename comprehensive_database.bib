	isbn = {0018-067X},
	issn = {0018-067X},
	journal = {Heredity},
	keywords = {Inbreeding depression,IxE,conservation genetics,environment interactions,fitness,genotype,inbreeding depression,stress},
	mendeley-tags = {Inbreeding depression,IxE},
	number = {3},
	pages = {235--242},
	pmid = {16077737},
	title = {{Inbreeding depression in benign and stressful environments.}},
