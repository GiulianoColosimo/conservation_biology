	doi = {10.1007/s10592-004-1981-z},
	file = {:Users/giuliano/Library/Application Support/Mendeley Desktop/Downloaded/Monsen, Blouin - 2004 - Extreme isolation by distance in a montane frog Rana cascadae.pdf:pdf},
	isbn = {1566-0621},
	issn = {15660621},
	journal = {Conservation Genetics},
	keywords = {Gene flow,Isolation by distance,Microsatellite,Rana cascadae},
	number = {6},
	pages = {827--835},
	pmid = {226236200007},
	title = {{Extreme isolation by distance in a montane frog Rana cascadae}},
