	file = {:Users/giuliano/Library/Application Support/Mendeley Desktop/Downloaded/Dudek et al. - 2016 - Distribution pattern and number of ticks on lizards.pdf:pdf},
	isbn = {1877-959X},
	issn = {18779603},
	journal = {Ticks and Tick-borne Diseases},
	keywords = {Dead ticks,Distribution,Ectoparasites,Ixodes ricinus,Lacerta agilis},
	number = {1},
	pages = {172--179},
	pmid = {26520053},
	publisher = {Elsevier GmbH.},
	title = {{Distribution pattern and number of ticks on lizards}},
	url = {http://dx.doi.org/10.1016/j.ttbdis.2015.10.014},
	volume = {7},
