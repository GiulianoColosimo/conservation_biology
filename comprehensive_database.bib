	file = {:Users/giuliano/Dropbox/Manuscripts/Litterature/PDFs/Warton{\_}Distance based multivariate analyses confound location and dispersion effect{\_}2012.pdf:pdf},
	isbn = {2041-210X},
	issn = {2041210X},
	journal = {Methods in Ecology and Evolution},
	keywords = {Bray-Curtis distance,Community composition,Generalised linear models,Mean-variance relationship,Multi-dimensional scaling,Multivariate analysis,PERMANOVA,SIMPER,Species-by-site data},
	number = {1},
	pages = {89--101},
	title = {{Distance-based multivariate analyses confound location and dispersion effects}},
	volume = {3},
	year = {2012},
	bdsk-url-1 = {https://doi.org/10.1111/j.2041-210X.2011.00127.x}}

