	isbn = {1091-6490 (Electronic)$\backslash$r0027-8424 (Linking)},
	issn = {1091-6490},
	journal = {Proceedings of the National Academy of Sciences of the United States of America},
	keywords = {Animals,Genetic Fitness,Genetic Fitness: genetics,Genetic Variation,Genetics, Population,Heterozygote,High-Throughput Nucleotide Sequencing,Illinois,Inbreeding,North Sea,Peromyscus,Peromyscus: genetics,Phoca,Phoca: genetics,Phoca: parasitology,Polymorphism, Single Nucleotide,Polymorphism, Single Nucleotide: genetics,Restriction Mapping},
	number = {10},
	pages = {3775--80},
