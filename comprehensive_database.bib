	keywords = {China,England,cereal aphids,insecticide resistance,population genetics},
	month = {sep},
	number = {18},
	pages = {9647--9662},
	title = {{Contrasting population structure and demographic history of cereal aphids in different environmental and agricultural landscapes}},
	url = {https://onlinelibrary.wiley.com/doi/10.1002/ece3.6565},
	volume = {10},
	year = {2020},
