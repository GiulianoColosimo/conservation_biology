	keywords = {Caribbean,Desecheo,Island biogeography,Islands,Mona,Mona Island,MtDNA,Sphaerodactylus,Sphaerodactylus.,mtDNA},
	mendeley-tags = {Islands,Mona Island,Sphaerodactylus,mtDNA},
	number = {13},
	pages = {4488--4500},
	pmid = {24340189},
	title = {{Colonization of islands in the Mona Passage by endemic dwarf geckoes (genus Sphaerodactylus) reconstructed with mitochondrial phylogeny}},
