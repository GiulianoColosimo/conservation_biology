	isbn = {1932-6203 (Electronic)$\backslash$r1932-6203 (Linking)},
	issn = {1932-6203},
	journal = {PLoS ONE},
	month = {apr},
	number = {4},
	pages = {e2018},
	pmid = {19194535},
	title = {{{\textless}i{\textgreater}Rickettsia{\textless}/i{\textgreater} Phylogenomics: Unwinding the Intricacies of Obligate Intracellular Life}},
