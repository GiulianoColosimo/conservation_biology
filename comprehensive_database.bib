	keywords = {Next-generation sequencing; Genetic inheritance; Family genetic inheritance; Genetic variants; Third generation sequencing},
	pages = {1250},
	title = {Next Generation Sequencing and Bioinformatics Analysis of Family Genetic Inheritance},
