	doi = {10.1007/s10592-010-0049-0},
	file = {:Users/giuliano/Library/Application Support/Mendeley Desktop/Downloaded/Bouzat - 2010 - Conservation genetics of population bottlenecks The role of chance, selection, and history.pdf:pdf},
	isbn = {1566-0621},
	issn = {15660621},
	journal = {Conservation Genetics},
	keywords = {Bottleneck,Conservation genetics,Extinction,Fitness,Genetic diversity,Inbreeding depression,Population bottlenecks,Population demography},
	mendeley-tags = {Bottleneck,Conservation genetics,Population demography},
	number = {2},
