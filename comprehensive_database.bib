	pmid = {25056064},
	publisher = {Nature Publishing Group},
	title = {{Climate change selects for heterozygosity in a declining fur seal population}},
	url = {http://www.nature.com/doifinder/10.1038/nature13542},
	volume = {511},
	year = {2014},
