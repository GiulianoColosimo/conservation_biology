	doi = {10.1186/1756-3305-7-93},
	file = {:Users/giuliano/Library/Application Support/Mendeley Desktop/Downloaded/Lv et al. - 2014 - Assessment of four DNA fragments ( COI , 16S rDNA , ITS2 , 12S rDNA ) for species identification of the Ixodida ( Aca.pdf:pdf},
	isbn = {1756-3305},
	issn = {Parasites {\&} Vectors},
	journal = {Parasites {\&} Vectors},
	keywords = {Ixodida, 12S, CO1X, ITS2, 16S},
	number = {1},
	pages = {1--11},
	pmid = {24589289},
	publisher = {Parasites {\&} Vectors},
	title = {{Assessment of four DNA fragments ( COI , 16S rDNA , ITS2 , 12S rDNA ) for species identification of the Ixodida ( Acari : Ixodida )}},
	url = {Parasites {\&} Vectors},
