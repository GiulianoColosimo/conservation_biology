	isbn = {1055-7903},
	issn = {10557903},
	journal = {Molecular Phylogenetics and Evolution},
	keywords = {Ameiva chrysolaema,Ameiva leberi,Biogeography,Pattern polymorphism,Phylogeography,Species complex},
