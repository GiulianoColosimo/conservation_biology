@article{Feighner1994,
	abstract = {Shipboard gravity and bathymetry data along with Geosat geoid data are used to explain the tectonic and structural evolution of the Galapagos region. The outboard parts of the southern and western escarpment are flexurally supported with an effective elastic thickness of {\~{}}12km. This area includes the large shield volcanoes of Fernandina and Isabela Islands. The central platform is weaker, with an elastic thickness of 6km or less, and close to Airy isostasy. The greatest depths to the Moho are located beneath eastern Isabela Island and the central platform. Thinner lithosphere in this region may account for the broad distribution of volcanoes, the extended period of eruption of the central volcanoes, and their reduced size. The transition from strong to weak lithosphere along the southern escarpment appears to be abrupt and can be best represented by a free end or faultlike discontinuity. -from Authors},
	author = {Feighner, Mark A. and Richards, Mark A.},
	date-added = {2021-07-01 12:00:14 +0200},
