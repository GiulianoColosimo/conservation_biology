	doi = {10.1016/j.biocon.2016.05.017},
	file = {:Users/giuliano/Library/Application Support/Mendeley Desktop/Downloaded/Auliya et al. - 2016 - Trade in live reptiles, its impact on wild populations, and the role of the European market.pdf:pdf},
	issn = {00063207},
	journal = {Biological Conservation},
	keywords = {European Union,Illegal,International law,Over-exploitation,Pet trade,Reptile diversity},
	number = {June},
	pages = {103--119},
	publisher = {Elsevier Ltd},
	title = {{Trade in live reptiles, its impact on wild populations, and the role of the European market}},
	url = {http://dx.doi.org/10.1016/j.biocon.2016.05.017},
	volume = {204},
